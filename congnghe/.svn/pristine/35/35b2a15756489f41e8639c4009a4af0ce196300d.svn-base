INSERT INTO `baiviet` (`stt`, `loaitin`, `tieude`, `tomtat`, `hinhanh`, `noidung`, `trangthai`) VALUES
(17, 'Tin tức', 'Hậu trường Galaxy Note 7 phát nổ: Một bộ phim hành động đúng nghĩa', '<p>V&agrave;o đầu năm 2016, những người đứng đầu của Samsung đ&atilde; nghe ng&oacute;ng được một nguồn tin quan trọng, nguồn tin đ&oacute; cho biết rằng phi&ecirc;n bản iPhone 7 của Apple sẽ kh&ocirc;ng c&oacute; qu&aacute; nhiều bất ngờ v&agrave; sự n&acirc;ng cấp đ&aacute;ng kể về mặt thiết kế lẫn cấu h&igrave;nh.</p>', 'website/view/tintuc/20160921023357note-7.jpg', '<p>Sau khi biết được rằng đối thủ lớn nhất của m&igrave;nh dường như "dặm ch&acirc;n tại chỗ",<a title="tin b&agrave;i về Samsung" type="tin b&agrave;i về Samsung" href="https://www.thegioididong.com/tin-tuc/tim-kiem?key=Samsung" target="_blank">Samsung</a>&nbsp;như "mở cờ trong bụng" v&agrave; biết rằng thời cơ để họ bứt ph&aacute; đ&atilde; tới.&nbsp;<span>Những người đứng đầu của "G&atilde; khổng lồ" H&agrave;n Quốc, kể cả gi&aacute;m đốc mảng điện thoại, &ocirc;ng D.J.Koh, đ&atilde; quyết định r&uacute;t ngắn thời gian ra mắt của chiếc Note 7, một chiếc phablet m&agrave; theo họ n&oacute; sẽ m&ecirc; hoặc người d&ugrave;ng v&agrave; gi&uacute;p Samsung chiếm lấy vị tr&iacute; dẫn đầu của Apple tại c&aacute;c thị trường lớn.</span></p>\r\n<p>V&igrave; thế Samsung đ&atilde; đốc th&uacute;c c&aacute;c đối t&aacute;c cung ứng linh kiện, buộc họ phải l&agrave;m việc cật lực hơn nữa nhằm gi&uacute;p cho Note 7 ra mắt sớm hơn chiếc iPhone 7, đồng thời vẫn phải t&iacute;ch hợp được những c&ocirc;ng nghệ h&agrave;ng đầu như m&agrave;n h&igrave;nh cong, qu&eacute;t mống mắt v&agrave; một vi&ecirc;n pin mạnh mẽ hơn, sạc nhanh hơn. Những điều m&agrave; Samsung đang toan t&iacute;nh thực hiện với Note 7 chỉ nhằm dập tắt những lời gi&egrave;m pha cho rằng họ chỉ biết sao ch&eacute;p "T&aacute;o khuyết" m&agrave; th&ocirc;i<span>.</span></p>', 1),
(18, 'Tin tức', '9 mẹo hay khi sử dụng HTC One A9', '<p><a title="HTC one A9" type="HTC one A9" href="http://www.thegioididong.com/dtdd/htc-one-a9" target="_blank">HTC One A9</a>&nbsp;l&agrave; một thiết bị rất đ&aacute;ng sử hữu của HTC. M&aacute;y c&oacute; một thiết kế đẹp, c&ugrave;ng cấu h&igrave;nh đủ đ&aacute;p ứng cho nhu cầu sử dụng của người ti&ecirc;u d&ugrave;ng. V&igrave; vậy, m&igrave;nh xin giới thiệu với c&aacute;c bạn 9 mẹo hay khi sử dụng chiếc smartphone n&agrave;y.</p>', 'website/view/tintuc/20160921024034a910.jpg', '<p>Xem th&ecirc;m:&nbsp;<a title="10 mẹo cực hay khi d&ugrave;ng Galaxy J7 Prime" type="10 mẹo cực hay khi d&ugrave;ng Galaxy J7 Prime" href="https://www.thegioididong.com/tin-tuc/10-meo-cuc-hay-khi-dung-galaxy-j7-prime-889597" target="_blank">10 mẹo cực hay khi d&ugrave;ng Galaxy J7 Prime</a></p>\r\n<h3><strong>1. Chạm 2 lần tắt mở s&aacute;ng m&agrave;n h&igrave;nh</strong></h3>\r\n<p>Bạn c&oacute; thể mở kh&oacute;a m&agrave;n h&igrave;nh nhanh v&agrave; tiết kiệm thời gian, bằng c&aacute;ch chạm 2 lần v&agrave;o m&agrave;n h&igrave;nh. Để bật t&iacute;nh năng n&agrave;y, bạn v&agrave;o&nbsp;<strong>C&agrave;i đặt &gt; Hiển thị v&agrave; Giao tiếp bằng cử chỉ &gt;&nbsp;K&iacute;ch hoạt th&ocirc;ng minh - Motion Launch</strong>. Tại đ&acirc;y, bạn chọn&nbsp;<strong>Chạm 2 lần để đ&aacute;nh thức v&agrave; ngủ.</strong></p>\r\n<div class="imgwrap"><img title="HTC One A9" src="https://cdn2.tgdd.vn/Files/2016/09/20/889805/a91.jpg" alt="HTC One A9" /></div>\r\n<h3><strong>2. Bật t&iacute;nh năng BoomSound</strong></h3>\r\n<p>C&ocirc;ng nghệ BoomSound l&agrave; một trong những c&ocirc;ng nghệ &acirc;m thanh "đỉnh" được t&iacute;ch hợp tr&ecirc;n nhiều d&ograve;ng Smartphone HTC, gi&uacute;p bạn tận hưởng &acirc;m thanh sống động ch&acirc;n thực khi xem phim, nghe nhạc, chơi game... V&igrave; thế bạn n&ecirc;n bật t&iacute;nh năng n&agrave;y.</p>\r\n<p>Bạn v&agrave;o&nbsp;<strong>C&agrave;i đặt</strong>&nbsp;v&agrave; bật&nbsp;<strong>HTC BoomSound với Dolby Audio</strong>.</p>', 1),
(19, 'Tin tức', 'iPhone 7 là chiếc điện thoại có màn hình LCD tốt nhất', '<p>Từ khi&nbsp;<a title="iPhone 7 " href="https://www.thegioididong.com/dtdd/apple-iphone-7" target="_blank">iPhone 7</a>&nbsp;ra mắt, đ&atilde; c&oacute; nhiều lời khen cũng như nhiều lời ch&ecirc; về mẫu điện thoại n&agrave;y. Nhưng d&ugrave; thế n&agrave;o đi nữa th&igrave; iPhone 7 vừa đạt được một v&agrave;i kỷ lục về<a title="m&agrave;n h&igrave;nh LCD" href="https://www.thegioididong.com/tin-tuc/tim-kiem?key=m%C3%A0n+h%C3%ACnh+LCD" target="_blank">m&agrave;n h&igrave;nh LCD</a>&nbsp;của n&oacute;.</p>', 'website/view/tintuc/20160921024121fa.jpg', '<p>Theo nguồn tin mới đ&acirc;y cho biết, m&agrave;n h&igrave;nh LCD của iPhone 7 đ&atilde; chiếm giữ một số kỷ lục như: M&agrave;n h&igrave;nh c&oacute; độ l&oacute;a cao nhất, M&agrave;n h&igrave;nh c&oacute; sự ch&ecirc;nh lệch m&agrave;u sắc nhỏ nhất của từng g&oacute;c nh&igrave;n, M&agrave;n h&igrave;nh LCD c&oacute; độ tương phản cao nhất, M&agrave;n h&igrave;nh c&oacute; độ ch&iacute;nh x&aacute;c về m&agrave;u sắc cao nhất. Đ&acirc;y tạm thời l&agrave; những kỷ lục m&agrave; m&agrave;n h&igrave;nh iPhone 7 đ&atilde; đạt được.</p>\r\n<p>Xem th&ecirc;m:&nbsp;<a title=" 8 điểm người d&ugrave;ng than phiền về iPhone 7 v&agrave; iPhone 7 Plus" href="https://www.thegioididong.com/tin-tuc/8-diem-nguoi-dung-than-phien-ve-iphone-7-va-iphone-7-plus-890092" target="_blank">8 điểm người d&ugrave;ng than phiền về iPhone 7 v&agrave; iPhone 7 Plus</a></p>\r\n<p>DisplayMate &ndash; một c&ocirc;ng ty chuy&ecirc;n đ&aacute;nh gi&aacute; c&aacute;c loại m&agrave;n h&igrave;nh rằng, thoạt nh&igrave;n 2 m&agrave;n h&igrave;nh của iPhone 6 v&agrave; iPhone 7 th&igrave; kh&ocirc;ng c&oacute; g&igrave; kh&aacute;c nhau. Tuy nhi&ecirc;n, m&agrave;n h&igrave;nh iPhone 7 c&oacute; tới 2 n&uacute;t Gamut m&agrave;u chuẩn trong khi iPhone 6 chỉ c&oacute; một m&agrave; th&ocirc;i. Gamut m&agrave;u của iPhone 6 từng được đ&aacute;nh gi&aacute; l&agrave; ch&iacute;nh x&aacute;c, như vậy iPhone 7 c&ograve;n rất ch&iacute;nh x&aacute;c hơn nữa.</p>\r\n<p>Nguồn tin c&ograve;n khẳng định th&ecirc;m: &ldquo;M&agrave;n h&igrave;nh iPhone 7 thật sự l&agrave; một m&agrave;n h&igrave;nh c&oacute; hiệu năng rất ấn tượng, đ&acirc;y l&agrave; một bản n&acirc;ng cấp của iPhone 6. C&oacute; thể n&oacute;i đ&acirc;y l&agrave; điện thoại c&oacute; m&agrave;n h&igrave;nh LCD tốt nhất hiện nay&rdquo;.</p>\r\n<p>Xem th&ecirc;m:&nbsp;<a title="Hiệu năng iPhone 7 nhanh hơn cả Macbook Air" href="https://www.thegioididong.com/tin-tuc/hieu-nang-iphone-7-nhanh-hon-macbook-air-888052" target="_blank">Hiệu năng iPhone 7 nhanh hơn cả Macbook Air</a></p>', 1),
(20, 'Tin tức', 'Loa di động JBL Charge 2 Plus hỗ trợ chống thấm nước giảm giá mạnh', '<p>Nếu bạn muốn sở hữu một chiếc&nbsp;<a title=" loa di động" href="https://www.thegioididong.com/loa-laptop" target="_blank">loa di động</a>&nbsp;với thiết kế đẹp, &acirc;m thanh chất lượng th&igrave; b&acirc;y giờ l&agrave; thời điểm đ&aacute;ng c&acirc;n nhắc. Mới đ&acirc;y, mẫu&nbsp;<a title="loa Bluetooth 6000mAH JBL Charge 2 Plus" href="https://www.thegioididong.com/loa-laptop/loa-bluetooth-6000mah-jbl-charge-2-plus" target="_blank">loa Bluetooth 6000mAH JBL Charge 2 Plus</a>&nbsp;đ&atilde; được giảm gi&aacute; kh&aacute; tốt.</p>', 'website/view/tintuc/20160921024206jbl-2.jpg', '<p>Theo đ&oacute;, trong chương tr&igrave;nh khuyến m&atilde;i mua online giảm gi&aacute; sốc, mẫu loa Bluetooth 6000mAH JBL Charge 2 Plus đ&atilde; được giảm gi&aacute; hơn 600 ng&agrave;n đồng, từ 3.6 triệu xuống c&ograve;n 2.99 triệu, lưu &yacute; l&agrave; thời gian chỉ c&ograve;n hơn 6 ng&agrave;y nữa th&ocirc;i nh&eacute;!</p>\r\n<div class="imgwrap"><img title="Loa di động JBL Charge 2 Plus hỗ trợ chống nước giảm gi&aacute; mạnh" src="https://cdn4.tgdd.vn/Files/2016/09/21/890231/jbl-2.jpg" alt="Loa di động JBL Charge 2 Plus hỗ trợ chống nước giảm gi&aacute; mạnh" /></div>\r\n<p>Được biết, đ&acirc;y l&agrave; loa di động JBL nổi tiếng với thiết kế h&igrave;nh trụ thời trang, m&agrave;u sắc trẻ trung. N&oacute; cũng hỗ trợ chống thấm nước, hỗ trợ bạn đi trong mưa nhỏ hay rửa khi bị bẩn.</p>\r\n<p>Ngo&agrave;i ra, loa Bluetooth 6000mAH JBL Charge 2 Plus c&ograve;n cho ph&eacute;p kết nối tới 3 thiết bị c&ugrave;ng l&uacute;c qua Bluetooth, c&oacute; thể trở th&agrave;nh một bộ sạc dự ph&ograve;ng cho thiết bị di động khi cần thiết bởi b&ecirc;n trong bộ loa n&agrave;y sở hữu thỏi pin dung lượng l&ecirc;n tới 6.000 mAh, c&oacute; thể ph&aacute;t nhạc li&ecirc;n tục trong 12 giờ.</p>', 1),
(21, 'Tin tức', 'Xiaomi chuẩn bị ra mắt smartphone giá rẻ màn hình 4 inch?', '<p><a title="Xiaomi" type="Xiaomi" href="https://www.thegioididong.com/tin-tuc/tim-kiem?key=Xiaomi" target="_blank">Xiaomi</a>&nbsp;đ&atilde; gửi thư mời ra mắt sản phẩm mới v&agrave;o ng&agrave;y 27/9 tới đ&acirc;y, ngo&agrave;i chiếc flagship Mi 5S th&igrave; mới đ&acirc;y th&ocirc;ng tin r&ograve; rỉ cho thấy h&atilde;ng c&oacute; thể c&ograve;n tr&igrave;nh l&agrave;ng một chiếc smartphone "mini" nữa.</p>', 'website/view/tintuc/20160921024235mi.jpg', '<p>Theo đ&oacute; một chiếc smartphone được tin l&agrave; của Xiaomi với m&agrave;n h&igrave;nh chỉ 4 inch đ&atilde; xuất hiện, qua h&igrave;nh ảnh r&ograve; rỉ c&oacute; thể thấy sản phẩm sở hữu n&uacute;t Home vật l&yacute; được thiết kế hơi d&agrave;i kh&aacute; giống như tr&ecirc;n Mi 5.</p>', 1),
(22, 'Tin tức', '“Quái vật” Le Pro 3 chính thức ra mắt với chip Snapdragon 821, giá quá rẻ', '<p>Sau một thời gian r&ograve; rỉ th&igrave; h&ocirc;m nay, &ldquo;qu&aacute;i vật&rdquo;&nbsp;<a title=" Le Pro 3" href="https://www.thegioididong.com/tin-tuc/tim-kiem?key=Le+Pro+3" target="_blank">Le Pro 3</a>&nbsp;đ&atilde; ch&iacute;nh thức ra mắt, sản phẩm chạy chip Snapdragon 821, bộ nhớ RAM l&ecirc;n đến 6 GB nhưng gi&aacute; b&aacute;n th&igrave; hết sức phải chăng.</p>', 'website/view/tintuc/20160921024307leeco-le-pro-3-announcement1.jpg', '<p>Xem th&ecirc;m:&nbsp;<a title="LeEco Le Pro 3 RAM 8 GB, chip Snapdragon 821 lộ ảnh ngo&agrave;i đời thực" href="https://www.thegioididong.com/tin-tuc/le-pro-3-ram-8-gb-chip-snapdragon-821-lo-anh-ngoai-doi-thuc-888373" target="_blank">LeEco Le Pro 3 RAM 8 GB, chip Snapdragon 821 lộ ảnh ngo&agrave;i đời thực</a></p>\r\n<p>Giống như Asus Zenfone 3 Deluxe, Le Pro 3 sở hữu bộ chip di động mạnh nhất nh&igrave; hiện nay tr&ecirc;n thị trường &ndash; Snapdragon 821 kết hợp c&ugrave;ng bộ nhớ RAM l&ecirc;n đến 6 GB LPDDR4. Trước đ&oacute; c&oacute; th&ocirc;ng tin cho rằng sẽ c&oacute; cả phi&ecirc;n bản 8 GB nhưng rất tiếc l&agrave; n&oacute; kh&ocirc;ng ra mắt. Nhưng m&igrave;nh nghĩ th&igrave; RAM 6 GB cũng l&agrave; qu&aacute; đủ rồi, phải kh&ocirc;ng n&agrave;o?</p>\r\n<div class="imgwrap"><img title="Qu&aacute;i vật Le Pro 3 ch&iacute;nh thức ra mắt với chip Snapdragon 821, gi&aacute; qu&aacute; rẻ" src="https://cdn1.tgdd.vn/Files/2016/09/21/890221/leeco-le-pro-3-announcement2.jpg" alt="Qu&aacute;i vật Le Pro 3 ch&iacute;nh thức ra mắt với chip Snapdragon 821, gi&aacute; qu&aacute; rẻ" /></div>\r\n<p>Thiết bị c&oacute; m&agrave;n h&igrave;nh 5.5 inch FullHD, camera 8/16 MP trước sau, pin dung lượng 4.070 mAh v&agrave; may mắn l&agrave; n&oacute; cũng hỗ trợ chế độ sạc nhanh Quick Charge 3.0. Mặc d&ugrave; dung lượng pin lớn như vậy nhưng m&aacute;y cũng chỉ mỏng 7.5 mm m&agrave; th&ocirc;i. Le Pro 3 chạy Android 6.0.1 tr&ecirc;n giao dện LeEco eUI.</p>\r\n<div class="imgwrap"><img title="Qu&aacute;i vật Le Pro 3 ch&iacute;nh thức ra mắt với chip Snapdragon 821, gi&aacute; qu&aacute; rẻ" src="https://cdn3.tgdd.vn/Files/2016/09/21/890221/leeco-le-pro-3-announcement3.jpg" alt="Qu&aacute;i vật Le Pro 3 ch&iacute;nh thức ra mắt với chip Snapdragon 821, gi&aacute; qu&aacute; rẻ" /></div>\r\n<p>Về gi&aacute; b&aacute;n, phi&ecirc;n bản RAM 4 GB + ROM 32 GB c&oacute; gi&aacute; 270 USD (khoảng 6 triệu đồng), bản RAM 4 GB + ROM 64 GB c&oacute; gi&aacute; 299 USD, bản RAM 6 GB + ROM 64 GB c&oacute; gi&aacute; 375 USD v&agrave; bản RAM 6 GB + ROM 128 GB c&oacute; gi&aacute; 450 USD.</p>', 1),
(23, 'Tin tức', '3 smartphone phiên bản màu đen đẹp không kém iPhone 7 Jet Black', '<p><a title="Xem tin về Apple iPhone 7" type="Xem tin về Apple iPhone 7" href="https://www.thegioididong.com/tin-tuc/tim-kiem?key=iPhone+7" target="_blank">iPhone 7</a>&nbsp;v&agrave; 7 Plus phi&ecirc;n bản m&agrave;u đen b&oacute;ng (Jet Black) đang nhận được rất nhiều sự quan t&acirc;m từ ph&iacute;a người d&ugrave;ng, nhất l&agrave; iFan. Nhưng nếu bạn cảm thấy gi&aacute; b&aacute;n của "dế" nh&agrave; T&aacute;o qu&aacute; cao, 3 đại diện trong b&agrave;i được xem l&agrave; sự thay thế ho&agrave;n hảo,</p>', 'website/view/tintuc/20160921024415xperia-xz-1.jpg', '<h3><strong>3.&nbsp;Asus ZenFone 3 ZE552KL</strong></h3>\r\n<p>L&yacute; do lọt TOP: ZenFone 3 sở hữu thiết kế nguy&ecirc;n khối nh&ocirc;m k&iacute;nh kết hợp, nh&igrave;n rất sang trọng v&agrave; chẳng kh&aacute;c g&igrave; một m&oacute;n trang sức đắt tiền. Về phần viền k&iacute;nh được l&agrave;m cong nhẹ 2.5D tạo cảm gi&aacute;c vuốt m&agrave;n h&igrave;nh rất đ&atilde; tay. C&ograve;n&nbsp;mặt lưng được trang tr&iacute; bằng những họa tiết đường tr&ograve;n đồng t&acirc;m bắt mắt c&ugrave;ng cụm cảm biến v&acirc;n tay một chạm hiện đại.</p>\r\n<p>Ưu điểm kh&aacute;c: Camera trước 8 MP + camera sau 16 MP quay phim 4K | RAM 4 GB + ROM 64 GB + Mạng&nbsp;<a title="Những điều bạn cần biết về c&ocirc;ng nghệ mạng di động 4G LTE" type="Những điều bạn cần biết về c&ocirc;ng nghệ mạng di động 4G LTE" href="https://www.thegioididong.com/tin-tuc/nhung-dieu-ban-can-biet-ve-cong-nghe-mang-di-dong-4g-lte-774427" target="_blank">4G</a>&nbsp;| Hỗ trợ&nbsp;<a title="Những điều cần biết về chuẩn kết nối mới USB type-C (USB-C)" type="Những điều cần biết về chuẩn kết nối mới USB type-C (USB-C)" href="https://www.thegioididong.com/tin-tuc/nhung-dieu-can-biet-ve-chuan-ket-noi-moi-usb-type--619776" target="_blank">USB Type-C</a>&nbsp;v&agrave; chạy&nbsp;<a title="Android 6 Marshmallow c&oacute; g&igrave; hot?" type="Android 6 Marshmallow c&oacute; g&igrave; hot?" href="https://www.thegioididong.com/hoi-dap/android-6-marshmallow-co-gi-hot-726146" target="_blank">Android 6.0 Marshmallow</a>.</p>', 1),
(28, 'Tin tức', 'Tại sao bạn nên mua smartphone chính hãng?', '<p>Chỉ khi so s&aacute;nh mặt-đối-mặt giữa&nbsp;<a title="h&agrave;ng ch&iacute;nh h&atilde;ng" type="h&agrave;ng ch&iacute;nh h&atilde;ng" href="https://www.thegioididong.com/tin-tuc/tim-kiem?key=h%C3%A0ng+ch%C3%ADnh+h%C3%A3ng" target="_blank">h&agrave;ng ch&iacute;nh h&atilde;ng</a>&nbsp;v&agrave;&nbsp;<a title="h&agrave;ng x&aacute;ch tay" type="h&agrave;ng x&aacute;ch tay" href="https://www.thegioididong.com/tin-tuc/tim-kiem?key=h%C3%A0ng+x%C3%A1ch+tay" target="_blank">h&agrave;ng x&aacute;ch tay</a>, ch&uacute;ng ta mới thấy được những ưu điểm của h&agrave;ng ch&iacute;nh h&atilde;ng mang lại.</p>', 'website/view/tintuc/20160928031521fa.jpg', '<p>Hiện nay, lu&ocirc;n c&oacute; hai loại h&agrave;ng h&oacute;a c&ugrave;ng song song lưu h&agrave;nh với nhau tr&ecirc;n thị trường&nbsp;<a title="smartphone " type="smartphone " href="https://www.thegioididong.com/dtdd" target="_blank">smartphone</a>&nbsp;Việt Nam, đ&oacute; l&agrave; h&agrave;ng ch&iacute;nh h&atilde;ng v&agrave; h&agrave;ng x&aacute;ch tay. C&ugrave;ng l&agrave; một chiếc m&aacute;y, nhưng do c&oacute; qu&aacute; nhiều mức gi&aacute; kh&aacute;c nhau đ&atilde; khiến cho người d&ugrave;ng tỏ ra l&uacute;ng t&uacute;ng trước việc chọn lựa mua h&agrave;ng.</p>\r\n<p>Để gi&uacute;p c&oacute; một c&aacute;i nh&igrave;n tổng quan về sự kh&aacute;c nhau, mời c&aacute;c bạn tham khảo Infographic giữa "H&agrave;ng ch&iacute;nh h&atilde;ng" v&agrave; "H&agrave;ng x&aacute;ch tay" dưới đ&acirc;y.</p>\r\n<div class="imgwrap"><img title="Tại sao bạn n&ecirc;n mua smartphone ch&iacute;nh h&atilde;ng" src="https://cdn1.tgdd.vn/Files/2016/09/28/892894/hangchinhhang.jpg" alt="Tại sao bạn n&ecirc;n mua smartphone ch&iacute;nh h&atilde;ng" /></div>', 1),
(29, 'Tin tức', 'Windows 10 Mobile Build 14936 đã khả dụng cho người dùng tải về', '<p>Rạng s&aacute;ng nay, Microsoft tung ra bản cập nhật 14936 cho người d&ugrave;ng tham gia chương tr&igrave;nh Insider ở nh&aacute;nh Fast Ring. Bản update hiện tại khả dụng cho cả người d&ugrave;ng Mobile v&agrave; PC.</p>', 'website/view/tintuc/20160929025807111.jpg', '<p>Theo changelog từ Microsoft th&igrave; kh&ocirc;ng c&oacute; t&iacute;nh năng mới được th&ecirc;m v&agrave;o, thay v&agrave;o đ&oacute; những lỗi tồn đọng tr&ecirc;n bản cập nhật trước đ&atilde; được khắc phục triệt để. Bạn cũng c&oacute; thể thao khảo danh s&aacute;ch đầy đủ những thay đổi trong bản&nbsp;14936 ngay b&ecirc;n dưới:</p>\r\n<h3><strong>Changelog Windows 10 Mobile Build 14936</strong></h3>\r\n<ul>\r\n<li>Lỗi kh&ocirc;ng hiển thị b&agrave;n ph&iacute;m nhập mật khẩu đ&atilde; được khắc phục.</li>\r\n<li>Lỗi kh&ocirc;ng nhận SIM đ&atilde; được khắc phục.</li>\r\n<li>T&iacute;nh năng&nbsp;hotspot đ&atilde; hoạt động trở lại b&igrave;nh thường.</li>\r\n<li>Khắc phục lỗi cập nhật tr&ecirc;n Lumia 650 (m&atilde; lỗi 0x80188308)</li>\r\n<li>Sửa lỗi kh&ocirc;ng thể cập nhật&nbsp;800703ed&nbsp;</li>\r\n<li>Trung t&acirc;m h&agrave;nh động kh&ocirc;ng c&ograve;n t&igrave;nh trạng thu lại khi bạn vuốt v&agrave;o v&ugrave;ng trống.</li>\r\n<li>Sửa lỗi th&ocirc;ng b&aacute;o lặp lại khi đ&atilde; xem.</li>\r\n<li>Khắc phục vấn đề lới Lumia 930, 1520 khi đang đ&agrave;m thoại m&agrave; cắm tai nghe th&igrave; &acirc;m thanh kh&ocirc;ng được chuyển v&agrave;o tai nghe.</li>\r\n</ul>\r\n<div class="imgwrap"><img title="1" src="https://cdn.tgdd.vn/Files/2016/09/29/893304/insider-program-rings-surface.jpg" alt="1" /></div>\r\n<h3><strong>Khuyễn m&atilde;i th&ecirc;m lỗi</strong></h3>\r\n<p>Giờ đ&acirc;y c&aacute;c ứng dụng&nbsp;WhatsApp, Facebook Messenger v&agrave; WeChat sẽ kh&ocirc;ng hoạt động nếu bạn c&agrave;i ch&uacute;ng tr&ecirc;n thẻ SD. Để khắc phục tạm thời lỗi n&agrave;y c&aacute;c bạn n&ecirc;n chuyển ch&uacute;ng v&agrave;o bộ nhớ m&aacute;y trong c&agrave;i đặt "Lưu trữ"</p>\r\n<p>Để nhận được bản cập nhật n&agrave;y c&aacute;c bạn v&agrave;o:</p>\r\n<ul>\r\n<li><strong>C&agrave;i đặt &gt; Cập nhật v&agrave; bảo mật &gt; Kiểm tra cập nhật &gt; Tải về</strong></li>\r\n</ul>', 1),
(33, 'Khuyến mãi', 'Bộ quà tặng siêu chất dành cho FAN OPPO', '<p><span>GAME &nbsp;</span><strong>&ldquo;GH&Eacute;P ẢNH VUI &ndash; KHUI QU&Agrave; CHẤT C&Ugrave;NG OPPO&rdquo;</strong></p>', 'website/view/tintuc/20161009121147i25.png', '<h3>TH&Ocirc;NG TIN CHƯƠNG TR&Igrave;NH</h3>\r\n<p>Mỗi &nbsp;SĐT c&oacute; 3 lượt chơi mỗi ng&agrave;y.</p>\r\n<p>Mỗi người ho&agrave;n th&agrave;nh gh&eacute;p 2 bức tranh, sau khi ho&agrave;n th&agrave;nh sẽ được mở qu&agrave; may mắn tr&uacute;ng thẻ c&agrave;o điện thoại.</p>\r\n<p>Cuối chương tr&igrave;nh tất cả người chơi gh&eacute;p đ&uacute;ng 2 bức ảnh được tham gia quay số để x&aacute;c nhận giải chung cuộc</p>\r\n<p><strong>C&Aacute;C BƯỚC THAM DỰ:</strong><br /><strong>Bước 1</strong>: Đăng nhập bằng Facebook/Gmail.<br /><strong>Bước 2</strong>: Gh&eacute;p ảnh&nbsp;<br /><strong>Bước 3</strong>: Chọn qu&agrave; may mắn.<br /><strong>Giải khuyến kh&iacute;ch</strong>:<br />- 700 thẻ c&agrave;o mệnh gi&aacute; 20.000đ<br />- 350 thẻ c&agrave;o mệnh g&aacute;i 50.000đ&nbsp;</p>\r\n<p>Thẻ c&agrave;o được gửi đến số điện thoại của kh&aacute;ch h&agrave;ng tr&uacute;ng thưởng trước 17:00 ng&agrave;y h&ocirc;m sau.&nbsp;<br /><strong>Giải chung cuộc:</strong><br />- 6 giải nhất mỗi giải 1 điện thoại OPPO F1s.<br />- 20 giải nh&igrave; mỗi giải 1 bộ gậy chụp ảnh 3 trong 1.<br />- 100 giải ba mỗi giải 1 phiếu mua h&agrave;ng trị gi&aacute; 200.000đ<br />H&igrave;nh thức x&aacute;c định người tr&uacute;ng giải chung cuộc:<br />Cuối chương tr&igrave;nh danh s&aacute;ch c&aacute;c người chơi gh&eacute;p đ&uacute;ng 2 bức ảnh trong chương tr&igrave;nh sẽ tổng hợp v&agrave; so s&aacute;nh với 6 số cuối số điện thoại (6 SĐT) với d&atilde;y số ngẫu nhi&ecirc;n gồm 6 chữ số (từ 0 - 999999), th&ocirc;ng qua trang web www.random.org.&nbsp;<br />Chọn ra 126 người c&oacute; kết quả TR&Ugrave;NG hoặc NHỎ HƠN gần nhất với d&atilde;y số ngẫu nhi&ecirc;n:&nbsp;<br />- 6 Người may mắn đầu ti&ecirc;n sẽ tr&uacute;ng Điện thoại OPPO F1s.<br />- 20 người tiếp theo tr&uacute;ng 1 bộ gậy chụp ảnh 3 trong 1.<br />- 100 người c&oacute; kết quả nhỏ hơn tiếp theo với d&atilde;y số ngẫu nhi&ecirc;n sẽ nhận ngay 100 phiếu mua h&agrave;ng trị gi&aacute; 200.000đ. ( &Aacute;p dụng mua phụ kiện mới - c&aacute;c sản phẩm Oppo).&nbsp;</p>\r\n<p>Phiếu mua h&agrave;ng sẽ gửi qua điện thoại của kh&aacute;ch h&agrave;ng trước 17:00 ng&agrave;y h&ocirc;m sau, t&iacute;nh từ thời gian c&ocirc;ng bố danh s&aacute;ch tr&uacute;ng giải, trường hợp tr&uacute;ng giải v&agrave;o ng&agrave;y thứ 7, chủ nhật sẽ gửi trước 12 giờ ng&agrave;y thứ 2 tuần kế tiếp.<br />Kh&aacute;ch h&agrave;ng tr&uacute;ng giải điện thoại OPPO F1s - 1 bộ gậy chụp ảnh 3 trong 1&nbsp;sẽ được ch&uacute;ng t&ocirc;i li&ecirc;n lạc trong v&ograve;ng 48h kể từ ng&agrave;y c&ocirc;ng bố kết quả.</p>\r\n<p>Quay số v&agrave;o l&uacute;c 09:00 ng&agrave;y 01/10/2016.<br />C&ocirc;ng bố danh s&aacute;ch tr&uacute;ng gỉai l&uacute;c 16:00 01/10/2016.<br />Trường hợp d&atilde;y số may mắn c&oacute; 5,4,3&hellip; chữ số, sẽ so s&aacute;nh 5,4,3,&hellip; số cuối SĐT của kh&aacute;ch h&agrave;ng.<br />Thời gian nhận giải chung cuộc: từ 04/10 - 10/10.</p>', 1);

