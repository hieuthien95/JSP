INSERT INTO `baiviet` (`stt`, `loaitin`, `tieude`, `tomtat`, `hinhanh`, `noidung`, `trangthai`) VALUES
(17, 'Tin tức', 'Hậu trường Galaxy Note 7 phát nổ: Một bộ phim hành động đúng nghĩa', '<p>V&agrave;o đầu năm 2016, những người đứng đầu của Samsung đ&atilde; nghe ng&oacute;ng được một nguồn tin quan trọng, nguồn tin đ&oacute; cho biết rằng phi&ecirc;n bản iPhone 7 của Apple sẽ kh&ocirc;ng c&oacute; qu&aacute; nhiều bất ngờ v&agrave; sự n&acirc;ng cấp đ&aacute;ng kể về mặt thiết kế lẫn cấu h&igrave;nh.</p>', 'website/view/tintuc/20160921023357note-7.jpg', '<p>Sau khi biết được rằng đối thủ lớn nhất của m&igrave;nh dường như "dặm ch&acirc;n tại chỗ",<a title="tin b&agrave;i về Samsung" type="tin b&agrave;i về Samsung" href="https://www.thegioididong.com/tin-tuc/tim-kiem?key=Samsung" target="_blank">Samsung</a>&nbsp;như "mở cờ trong bụng" v&agrave; biết rằng thời cơ để họ bứt ph&aacute; đ&atilde; tới.&nbsp;<span>Những người đứng đầu của "G&atilde; khổng lồ" H&agrave;n Quốc, kể cả gi&aacute;m đốc mảng điện thoại, &ocirc;ng D.J.Koh, đ&atilde; quyết định r&uacute;t ngắn thời gian ra mắt của chiếc Note 7, một chiếc phablet m&agrave; theo họ n&oacute; sẽ m&ecirc; hoặc người d&ugrave;ng v&agrave; gi&uacute;p Samsung chiếm lấy vị tr&iacute; dẫn đầu của Apple tại c&aacute;c thị trường lớn.</span></p>\r\n<p>V&igrave; thế Samsung đ&atilde; đốc th&uacute;c c&aacute;c đối t&aacute;c cung ứng linh kiện, buộc họ phải l&agrave;m việc cật lực hơn nữa nhằm gi&uacute;p cho Note 7 ra mắt sớm hơn chiếc iPhone 7, đồng thời vẫn phải t&iacute;ch hợp được những c&ocirc;ng nghệ h&agrave;ng đầu như m&agrave;n h&igrave;nh cong, qu&eacute;t mống mắt v&agrave; một vi&ecirc;n pin mạnh mẽ hơn, sạc nhanh hơn. Những điều m&agrave; Samsung đang toan t&iacute;nh thực hiện với Note 7 chỉ nhằm dập tắt những lời gi&egrave;m pha cho rằng họ chỉ biết sao ch&eacute;p "T&aacute;o khuyết" m&agrave; th&ocirc;i<span>.</span></p>', 1),
(18, 'Tin tức', '9 mẹo hay khi sử dụng HTC One A9', '<p><a title="HTC one A9" type="HTC one A9" href="http://www.thegioididong.com/dtdd/htc-one-a9" target="_blank">HTC One A9</a>&nbsp;l&agrave; một thiết bị rất đ&aacute;ng sử hữu của HTC. M&aacute;y c&oacute; một thiết kế đẹp, c&ugrave;ng cấu h&igrave;nh đủ đ&aacute;p ứng cho nhu cầu sử dụng của người ti&ecirc;u d&ugrave;ng. V&igrave; vậy, m&igrave;nh xin giới thiệu với c&aacute;c bạn 9 mẹo hay khi sử dụng chiếc smartphone n&agrave;y.</p>', 'website/view/tintuc/20160921024034a910.jpg', '<p>Xem th&ecirc;m:&nbsp;<a title="10 mẹo cực hay khi d&ugrave;ng Galaxy J7 Prime" type="10 mẹo cực hay khi d&ugrave;ng Galaxy J7 Prime" href="https://www.thegioididong.com/tin-tuc/10-meo-cuc-hay-khi-dung-galaxy-j7-prime-889597" target="_blank">10 mẹo cực hay khi d&ugrave;ng Galaxy J7 Prime</a></p>\r\n<h3><strong>1. Chạm 2 lần tắt mở s&aacute;ng m&agrave;n h&igrave;nh</strong></h3>\r\n<p>Bạn c&oacute; thể mở kh&oacute;a m&agrave;n h&igrave;nh nhanh v&agrave; tiết kiệm thời gian, bằng c&aacute;ch chạm 2 lần v&agrave;o m&agrave;n h&igrave;nh. Để bật t&iacute;nh năng n&agrave;y, bạn v&agrave;o&nbsp;<strong>C&agrave;i đặt &gt; Hiển thị v&agrave; Giao tiếp bằng cử chỉ &gt;&nbsp;K&iacute;ch hoạt th&ocirc;ng minh - Motion Launch</strong>. Tại đ&acirc;y, bạn chọn&nbsp;<strong>Chạm 2 lần để đ&aacute;nh thức v&agrave; ngủ.</strong></p>\r\n<div class="imgwrap"><img title="HTC One A9" src="https://cdn2.tgdd.vn/Files/2016/09/20/889805/a91.jpg" alt="HTC One A9" /></div>\r\n<h3><strong>2. Bật t&iacute;nh năng BoomSound</strong></h3>\r\n<p>C&ocirc;ng nghệ BoomSound l&agrave; một trong những c&ocirc;ng nghệ &acirc;m thanh "đỉnh" được t&iacute;ch hợp tr&ecirc;n nhiều d&ograve;ng Smartphone HTC, gi&uacute;p bạn tận hưởng &acirc;m thanh sống động ch&acirc;n thực khi xem phim, nghe nhạc, chơi game... V&igrave; thế bạn n&ecirc;n bật t&iacute;nh năng n&agrave;y.</p>\r\n<p>Bạn v&agrave;o&nbsp;<strong>C&agrave;i đặt</strong>&nbsp;v&agrave; bật&nbsp;<strong>HTC BoomSound với Dolby Audio</strong>.</p>', 1),
(19, 'Tin tức', 'iPhone 7 là chiếc điện thoại có màn hình LCD tốt nhất', '<p>Từ khi&nbsp;<a title="iPhone 7 " href="https://www.thegioididong.com/dtdd/apple-iphone-7" target="_blank">iPhone 7</a>&nbsp;ra mắt, đ&atilde; c&oacute; nhiều lời khen cũng như nhiều lời ch&ecirc; về mẫu điện thoại n&agrave;y. Nhưng d&ugrave; thế n&agrave;o đi nữa th&igrave; iPhone 7 vừa đạt được một v&agrave;i kỷ lục về<a title="m&agrave;n h&igrave;nh LCD" href="https://www.thegioididong.com/tin-tuc/tim-kiem?key=m%C3%A0n+h%C3%ACnh+LCD" target="_blank">m&agrave;n h&igrave;nh LCD</a>&nbsp;của n&oacute;.</p>', 'website/view/tintuc/20160921024121fa.jpg', '<p>Theo nguồn tin mới đ&acirc;y cho biết, m&agrave;n h&igrave;nh LCD của iPhone 7 đ&atilde; chiếm giữ một số kỷ lục như: M&agrave;n h&igrave;nh c&oacute; độ l&oacute;a cao nhất, M&agrave;n h&igrave;nh c&oacute; sự ch&ecirc;nh lệch m&agrave;u sắc nhỏ nhất của từng g&oacute;c nh&igrave;n, M&agrave;n h&igrave;nh LCD c&oacute; độ tương phản cao nhất, M&agrave;n h&igrave;nh c&oacute; độ ch&iacute;nh x&aacute;c về m&agrave;u sắc cao nhất. Đ&acirc;y tạm thời l&agrave; những kỷ lục m&agrave; m&agrave;n h&igrave;nh iPhone 7 đ&atilde; đạt được.</p>\r\n<p>Xem th&ecirc;m:&nbsp;<a title=" 8 điểm người d&ugrave;ng than phiền về iPhone 7 v&agrave; iPhone 7 Plus" href="https://www.thegioididong.com/tin-tuc/8-diem-nguoi-dung-than-phien-ve-iphone-7-va-iphone-7-plus-890092" target="_blank">8 điểm người d&ugrave;ng than phiền về iPhone 7 v&agrave; iPhone 7 Plus</a></p>\r\n<p>DisplayMate &ndash; một c&ocirc;ng ty chuy&ecirc;n đ&aacute;nh gi&aacute; c&aacute;c loại m&agrave;n h&igrave;nh rằng, thoạt nh&igrave;n 2 m&agrave;n h&igrave;nh của iPhone 6 v&agrave; iPhone 7 th&igrave; kh&ocirc;ng c&oacute; g&igrave; kh&aacute;c nhau. Tuy nhi&ecirc;n, m&agrave;n h&igrave;nh iPhone 7 c&oacute; tới 2 n&uacute;t Gamut m&agrave;u chuẩn trong khi iPhone 6 chỉ c&oacute; một m&agrave; th&ocirc;i. Gamut m&agrave;u của iPhone 6 từng được đ&aacute;nh gi&aacute; l&agrave; ch&iacute;nh x&aacute;c, như vậy iPhone 7 c&ograve;n rất ch&iacute;nh x&aacute;c hơn nữa.</p>\r\n<p>Nguồn tin c&ograve;n khẳng định th&ecirc;m: &ldquo;M&agrave;n h&igrave;nh iPhone 7 thật sự l&agrave; một m&agrave;n h&igrave;nh c&oacute; hiệu năng rất ấn tượng, đ&acirc;y l&agrave; một bản n&acirc;ng cấp của iPhone 6. C&oacute; thể n&oacute;i đ&acirc;y l&agrave; điện thoại c&oacute; m&agrave;n h&igrave;nh LCD tốt nhất hiện nay&rdquo;.</p>\r\n<p>Xem th&ecirc;m:&nbsp;<a title="Hiệu năng iPhone 7 nhanh hơn cả Macbook Air" href="https://www.thegioididong.com/tin-tuc/hieu-nang-iphone-7-nhanh-hon-macbook-air-888052" target="_blank">Hiệu năng iPhone 7 nhanh hơn cả Macbook Air</a></p>', 1),
(20, 'Tin tức', 'Loa di động JBL Charge 2 Plus hỗ trợ chống thấm nước giảm giá mạnh', '<p>Nếu bạn muốn sở hữu một chiếc&nbsp;<a title=" loa di động" href="https://www.thegioididong.com/loa-laptop" target="_blank">loa di động</a>&nbsp;với thiết kế đẹp, &acirc;m thanh chất lượng th&igrave; b&acirc;y giờ l&agrave; thời điểm đ&aacute;ng c&acirc;n nhắc. Mới đ&acirc;y, mẫu&nbsp;<a title="loa Bluetooth 6000mAH JBL Charge 2 Plus" href="https://www.thegioididong.com/loa-laptop/loa-bluetooth-6000mah-jbl-charge-2-plus" target="_blank">loa Bluetooth 6000mAH JBL Charge 2 Plus</a>&nbsp;đ&atilde; được giảm gi&aacute; kh&aacute; tốt.</p>', 'website/view/tintuc/20160921024206jbl-2.jpg', '<p>Theo đ&oacute;, trong chương tr&igrave;nh khuyến m&atilde;i mua online giảm gi&aacute; sốc, mẫu loa Bluetooth 6000mAH JBL Charge 2 Plus đ&atilde; được giảm gi&aacute; hơn 600 ng&agrave;n đồng, từ 3.6 triệu xuống c&ograve;n 2.99 triệu, lưu &yacute; l&agrave; thời gian chỉ c&ograve;n hơn 6 ng&agrave;y nữa th&ocirc;i nh&eacute;!</p>\r\n<div class="imgwrap"><img title="Loa di động JBL Charge 2 Plus hỗ trợ chống nước giảm gi&aacute; mạnh" src="https://cdn4.tgdd.vn/Files/2016/09/21/890231/jbl-2.jpg" alt="Loa di động JBL Charge 2 Plus hỗ trợ chống nước giảm gi&aacute; mạnh" /></div>\r\n<p>Được biết, đ&acirc;y l&agrave; loa di động JBL nổi tiếng với thiết kế h&igrave;nh trụ thời trang, m&agrave;u sắc trẻ trung. N&oacute; cũng hỗ trợ chống thấm nước, hỗ trợ bạn đi trong mưa nhỏ hay rửa khi bị bẩn.</p>\r\n<p>Ngo&agrave;i ra, loa Bluetooth 6000mAH JBL Charge 2 Plus c&ograve;n cho ph&eacute;p kết nối tới 3 thiết bị c&ugrave;ng l&uacute;c qua Bluetooth, c&oacute; thể trở th&agrave;nh một bộ sạc dự ph&ograve;ng cho thiết bị di động khi cần thiết bởi b&ecirc;n trong bộ loa n&agrave;y sở hữu thỏi pin dung lượng l&ecirc;n tới 6.000 mAh, c&oacute; thể ph&aacute;t nhạc li&ecirc;n tục trong 12 giờ.</p>', 1),
(21, 'Tin tức', 'Xiaomi chuẩn bị ra mắt smartphone giá rẻ màn hình 4 inch?', '<p><a title="Xiaomi" type="Xiaomi" href="https://www.thegioididong.com/tin-tuc/tim-kiem?key=Xiaomi" target="_blank">Xiaomi</a>&nbsp;đ&atilde; gửi thư mời ra mắt sản phẩm mới v&agrave;o ng&agrave;y 27/9 tới đ&acirc;y, ngo&agrave;i chiếc flagship Mi 5S th&igrave; mới đ&acirc;y th&ocirc;ng tin r&ograve; rỉ cho thấy h&atilde;ng c&oacute; thể c&ograve;n tr&igrave;nh l&agrave;ng một chiếc smartphone "mini" nữa.</p>', 'website/view/tintuc/20160921024235mi.jpg', '<p>Theo đ&oacute; một chiếc smartphone được tin l&agrave; của Xiaomi với m&agrave;n h&igrave;nh chỉ 4 inch đ&atilde; xuất hiện, qua h&igrave;nh ảnh r&ograve; rỉ c&oacute; thể thấy sản phẩm sở hữu n&uacute;t Home vật l&yacute; được thiết kế hơi d&agrave;i kh&aacute; giống như tr&ecirc;n Mi 5.</p>', 1),
(22, 'Tin tức', '“Quái vật” Le Pro 3 chính thức ra mắt với chip Snapdragon 821, giá quá rẻ', '<p>Sau một thời gian r&ograve; rỉ th&igrave; h&ocirc;m nay, &ldquo;qu&aacute;i vật&rdquo;&nbsp;<a title=" Le Pro 3" href="https://www.thegioididong.com/tin-tuc/tim-kiem?key=Le+Pro+3" target="_blank">Le Pro 3</a>&nbsp;đ&atilde; ch&iacute;nh thức ra mắt, sản phẩm chạy chip Snapdragon 821, bộ nhớ RAM l&ecirc;n đến 6 GB nhưng gi&aacute; b&aacute;n th&igrave; hết sức phải chăng.</p>', 'website/view/tintuc/20160921024307leeco-le-pro-3-announcement1.jpg', '<p>Xem th&ecirc;m:&nbsp;<a title="LeEco Le Pro 3 RAM 8 GB, chip Snapdragon 821 lộ ảnh ngo&agrave;i đời thực" href="https://www.thegioididong.com/tin-tuc/le-pro-3-ram-8-gb-chip-snapdragon-821-lo-anh-ngoai-doi-thuc-888373" target="_blank">LeEco Le Pro 3 RAM 8 GB, chip Snapdragon 821 lộ ảnh ngo&agrave;i đời thực</a></p>\r\n<p>Giống như Asus Zenfone 3 Deluxe, Le Pro 3 sở hữu bộ chip di động mạnh nhất nh&igrave; hiện nay tr&ecirc;n thị trường &ndash; Snapdragon 821 kết hợp c&ugrave;ng bộ nhớ RAM l&ecirc;n đến 6 GB LPDDR4. Trước đ&oacute; c&oacute; th&ocirc;ng tin cho rằng sẽ c&oacute; cả phi&ecirc;n bản 8 GB nhưng rất tiếc l&agrave; n&oacute; kh&ocirc;ng ra mắt. Nhưng m&igrave;nh nghĩ th&igrave; RAM 6 GB cũng l&agrave; qu&aacute; đủ rồi, phải kh&ocirc;ng n&agrave;o?</p>\r\n<div class="imgwrap"><img title="Qu&aacute;i vật Le Pro 3 ch&iacute;nh thức ra mắt với chip Snapdragon 821, gi&aacute; qu&aacute; rẻ" src="https://cdn1.tgdd.vn/Files/2016/09/21/890221/leeco-le-pro-3-announcement2.jpg" alt="Qu&aacute;i vật Le Pro 3 ch&iacute;nh thức ra mắt với chip Snapdragon 821, gi&aacute; qu&aacute; rẻ" /></div>\r\n<p>Thiết bị c&oacute; m&agrave;n h&igrave;nh 5.5 inch FullHD, camera 8/16 MP trước sau, pin dung lượng 4.070 mAh v&agrave; may mắn l&agrave; n&oacute; cũng hỗ trợ chế độ sạc nhanh Quick Charge 3.0. Mặc d&ugrave; dung lượng pin lớn như vậy nhưng m&aacute;y cũng chỉ mỏng 7.5 mm m&agrave; th&ocirc;i. Le Pro 3 chạy Android 6.0.1 tr&ecirc;n giao dện LeEco eUI.</p>\r\n<div class="imgwrap"><img title="Qu&aacute;i vật Le Pro 3 ch&iacute;nh thức ra mắt với chip Snapdragon 821, gi&aacute; qu&aacute; rẻ" src="https://cdn3.tgdd.vn/Files/2016/09/21/890221/leeco-le-pro-3-announcement3.jpg" alt="Qu&aacute;i vật Le Pro 3 ch&iacute;nh thức ra mắt với chip Snapdragon 821, gi&aacute; qu&aacute; rẻ" /></div>\r\n<p>Về gi&aacute; b&aacute;n, phi&ecirc;n bản RAM 4 GB + ROM 32 GB c&oacute; gi&aacute; 270 USD (khoảng 6 triệu đồng), bản RAM 4 GB + ROM 64 GB c&oacute; gi&aacute; 299 USD, bản RAM 6 GB + ROM 64 GB c&oacute; gi&aacute; 375 USD v&agrave; bản RAM 6 GB + ROM 128 GB c&oacute; gi&aacute; 450 USD.</p>', 1),
(23, 'Tin tức', '3 smartphone phiên bản màu đen đẹp không kém iPhone 7 Jet Black', '<p><a title="Xem tin về Apple iPhone 7" type="Xem tin về Apple iPhone 7" href="https://www.thegioididong.com/tin-tuc/tim-kiem?key=iPhone+7" target="_blank">iPhone 7</a>&nbsp;v&agrave; 7 Plus phi&ecirc;n bản m&agrave;u đen b&oacute;ng (Jet Black) đang nhận được rất nhiều sự quan t&acirc;m từ ph&iacute;a người d&ugrave;ng, nhất l&agrave; iFan. Nhưng nếu bạn cảm thấy gi&aacute; b&aacute;n của "dế" nh&agrave; T&aacute;o qu&aacute; cao, 3 đại diện trong b&agrave;i được xem l&agrave; sự thay thế ho&agrave;n hảo,</p>', 'website/view/tintuc/20160921024415xperia-xz-1.jpg', '<h3><strong>3.&nbsp;Asus ZenFone 3 ZE552KL</strong></h3>\r\n<p>L&yacute; do lọt TOP: ZenFone 3 sở hữu thiết kế nguy&ecirc;n khối nh&ocirc;m k&iacute;nh kết hợp, nh&igrave;n rất sang trọng v&agrave; chẳng kh&aacute;c g&igrave; một m&oacute;n trang sức đắt tiền. Về phần viền k&iacute;nh được l&agrave;m cong nhẹ 2.5D tạo cảm gi&aacute;c vuốt m&agrave;n h&igrave;nh rất đ&atilde; tay. C&ograve;n&nbsp;mặt lưng được trang tr&iacute; bằng những họa tiết đường tr&ograve;n đồng t&acirc;m bắt mắt c&ugrave;ng cụm cảm biến v&acirc;n tay một chạm hiện đại.</p>\r\n<p>Ưu điểm kh&aacute;c: Camera trước 8 MP + camera sau 16 MP quay phim 4K | RAM 4 GB + ROM 64 GB + Mạng&nbsp;<a title="Những điều bạn cần biết về c&ocirc;ng nghệ mạng di động 4G LTE" type="Những điều bạn cần biết về c&ocirc;ng nghệ mạng di động 4G LTE" href="https://www.thegioididong.com/tin-tuc/nhung-dieu-ban-can-biet-ve-cong-nghe-mang-di-dong-4g-lte-774427" target="_blank">4G</a>&nbsp;| Hỗ trợ&nbsp;<a title="Những điều cần biết về chuẩn kết nối mới USB type-C (USB-C)" type="Những điều cần biết về chuẩn kết nối mới USB type-C (USB-C)" href="https://www.thegioididong.com/tin-tuc/nhung-dieu-can-biet-ve-chuan-ket-noi-moi-usb-type--619776" target="_blank">USB Type-C</a>&nbsp;v&agrave; chạy&nbsp;<a title="Android 6 Marshmallow c&oacute; g&igrave; hot?" type="Android 6 Marshmallow c&oacute; g&igrave; hot?" href="https://www.thegioididong.com/hoi-dap/android-6-marshmallow-co-gi-hot-726146" target="_blank">Android 6.0 Marshmallow</a>.</p>', 1),
(28, 'Tin tức', 'Tại sao bạn nên mua smartphone chính hãng?', '<p>Chỉ khi so s&aacute;nh mặt-đối-mặt giữa&nbsp;<a title="h&agrave;ng ch&iacute;nh h&atilde;ng" type="h&agrave;ng ch&iacute;nh h&atilde;ng" href="https://www.thegioididong.com/tin-tuc/tim-kiem?key=h%C3%A0ng+ch%C3%ADnh+h%C3%A3ng" target="_blank">h&agrave;ng ch&iacute;nh h&atilde;ng</a>&nbsp;v&agrave;&nbsp;<a title="h&agrave;ng x&aacute;ch tay" type="h&agrave;ng x&aacute;ch tay" href="https://www.thegioididong.com/tin-tuc/tim-kiem?key=h%C3%A0ng+x%C3%A1ch+tay" target="_blank">h&agrave;ng x&aacute;ch tay</a>, ch&uacute;ng ta mới thấy được những ưu điểm của h&agrave;ng ch&iacute;nh h&atilde;ng mang lại.</p>', 'website/view/tintuc/20160928031521fa.jpg', '<p>Hiện nay, lu&ocirc;n c&oacute; hai loại h&agrave;ng h&oacute;a c&ugrave;ng song song lưu h&agrave;nh với nhau tr&ecirc;n thị trường&nbsp;<a title="smartphone " type="smartphone " href="https://www.thegioididong.com/dtdd" target="_blank">smartphone</a>&nbsp;Việt Nam, đ&oacute; l&agrave; h&agrave;ng ch&iacute;nh h&atilde;ng v&agrave; h&agrave;ng x&aacute;ch tay. C&ugrave;ng l&agrave; một chiếc m&aacute;y, nhưng do c&oacute; qu&aacute; nhiều mức gi&aacute; kh&aacute;c nhau đ&atilde; khiến cho người d&ugrave;ng tỏ ra l&uacute;ng t&uacute;ng trước việc chọn lựa mua h&agrave;ng.</p>\r\n<p>Để gi&uacute;p c&oacute; một c&aacute;i nh&igrave;n tổng quan về sự kh&aacute;c nhau, mời c&aacute;c bạn tham khảo Infographic giữa "H&agrave;ng ch&iacute;nh h&atilde;ng" v&agrave; "H&agrave;ng x&aacute;ch tay" dưới đ&acirc;y.</p>\r\n<div class="imgwrap"><img title="Tại sao bạn n&ecirc;n mua smartphone ch&iacute;nh h&atilde;ng" src="https://cdn1.tgdd.vn/Files/2016/09/28/892894/hangchinhhang.jpg" alt="Tại sao bạn n&ecirc;n mua smartphone ch&iacute;nh h&atilde;ng" /></div>', 1),
(29, 'Tin tức', 'Windows 10 Mobile Build 14936 đã khả dụng cho người dùng tải về', '<p>Rạng s&aacute;ng nay, Microsoft tung ra bản cập nhật 14936 cho người d&ugrave;ng tham gia chương tr&igrave;nh Insider ở nh&aacute;nh Fast Ring. Bản update hiện tại khả dụng cho cả người d&ugrave;ng Mobile v&agrave; PC.</p>', 'website/view/tintuc/20160929025807111.jpg', '<p>Theo changelog từ Microsoft th&igrave; kh&ocirc;ng c&oacute; t&iacute;nh năng mới được th&ecirc;m v&agrave;o, thay v&agrave;o đ&oacute; những lỗi tồn đọng tr&ecirc;n bản cập nhật trước đ&atilde; được khắc phục triệt để. Bạn cũng c&oacute; thể thao khảo danh s&aacute;ch đầy đủ những thay đổi trong bản&nbsp;14936 ngay b&ecirc;n dưới:</p>\r\n<h3><strong>Changelog Windows 10 Mobile Build 14936</strong></h3>\r\n<ul>\r\n<li>Lỗi kh&ocirc;ng hiển thị b&agrave;n ph&iacute;m nhập mật khẩu đ&atilde; được khắc phục.</li>\r\n<li>Lỗi kh&ocirc;ng nhận SIM đ&atilde; được khắc phục.</li>\r\n<li>T&iacute;nh năng&nbsp;hotspot đ&atilde; hoạt động trở lại b&igrave;nh thường.</li>\r\n<li>Khắc phục lỗi cập nhật tr&ecirc;n Lumia 650 (m&atilde; lỗi 0x80188308)</li>\r\n<li>Sửa lỗi kh&ocirc;ng thể cập nhật&nbsp;800703ed&nbsp;</li>\r\n<li>Trung t&acirc;m h&agrave;nh động kh&ocirc;ng c&ograve;n t&igrave;nh trạng thu lại khi bạn vuốt v&agrave;o v&ugrave;ng trống.</li>\r\n<li>Sửa lỗi th&ocirc;ng b&aacute;o lặp lại khi đ&atilde; xem.</li>\r\n<li>Khắc phục vấn đề lới Lumia 930, 1520 khi đang đ&agrave;m thoại m&agrave; cắm tai nghe th&igrave; &acirc;m thanh kh&ocirc;ng được chuyển v&agrave;o tai nghe.</li>\r\n</ul>\r\n<div class="imgwrap"><img title="1" src="https://cdn.tgdd.vn/Files/2016/09/29/893304/insider-program-rings-surface.jpg" alt="1" /></div>\r\n<h3><strong>Khuyễn m&atilde;i th&ecirc;m lỗi</strong></h3>\r\n<p>Giờ đ&acirc;y c&aacute;c ứng dụng&nbsp;WhatsApp, Facebook Messenger v&agrave; WeChat sẽ kh&ocirc;ng hoạt động nếu bạn c&agrave;i ch&uacute;ng tr&ecirc;n thẻ SD. Để khắc phục tạm thời lỗi n&agrave;y c&aacute;c bạn n&ecirc;n chuyển ch&uacute;ng v&agrave;o bộ nhớ m&aacute;y trong c&agrave;i đặt "Lưu trữ"</p>\r\n<p>Để nhận được bản cập nhật n&agrave;y c&aacute;c bạn v&agrave;o:</p>\r\n<ul>\r\n<li><strong>C&agrave;i đặt &gt; Cập nhật v&agrave; bảo mật &gt; Kiểm tra cập nhật &gt; Tải về</strong></li>\r\n</ul>', 1),
(33, 'Khuyến mãi', 'Bộ quà tặng siêu chất dành cho FAN OPPO', '<p><span>GAME &nbsp;</span><strong>&ldquo;GH&Eacute;P ẢNH VUI &ndash; KHUI QU&Agrave; CHẤT C&Ugrave;NG OPPO&rdquo;</strong></p>', 'website/view/tintuc/20161009121147i25.png', '<h3>TH&Ocirc;NG TIN CHƯƠNG TR&Igrave;NH</h3>\r\n<p>Mỗi &nbsp;SĐT c&oacute; 3 lượt chơi mỗi ng&agrave;y.</p>\r\n<p>Mỗi người ho&agrave;n th&agrave;nh gh&eacute;p 2 bức tranh, sau khi ho&agrave;n th&agrave;nh sẽ được mở qu&agrave; may mắn tr&uacute;ng thẻ c&agrave;o điện thoại.</p>\r\n<p>Cuối chương tr&igrave;nh tất cả người chơi gh&eacute;p đ&uacute;ng 2 bức ảnh được tham gia quay số để x&aacute;c nhận giải chung cuộc</p>\r\n<p><strong>C&Aacute;C BƯỚC THAM DỰ:</strong><br /><strong>Bước 1</strong>: Đăng nhập bằng Facebook/Gmail.<br /><strong>Bước 2</strong>: Gh&eacute;p ảnh&nbsp;<br /><strong>Bước 3</strong>: Chọn qu&agrave; may mắn.<br /><strong>Giải khuyến kh&iacute;ch</strong>:<br />- 700 thẻ c&agrave;o mệnh gi&aacute; 20.000đ<br />- 350 thẻ c&agrave;o mệnh g&aacute;i 50.000đ&nbsp;</p>\r\n<p>Thẻ c&agrave;o được gửi đến số điện thoại của kh&aacute;ch h&agrave;ng tr&uacute;ng thưởng trước 17:00 ng&agrave;y h&ocirc;m sau.&nbsp;<br /><strong>Giải chung cuộc:</strong><br />- 6 giải nhất mỗi giải 1 điện thoại OPPO F1s.<br />- 20 giải nh&igrave; mỗi giải 1 bộ gậy chụp ảnh 3 trong 1.<br />- 100 giải ba mỗi giải 1 phiếu mua h&agrave;ng trị gi&aacute; 200.000đ<br />H&igrave;nh thức x&aacute;c định người tr&uacute;ng giải chung cuộc:<br />Cuối chương tr&igrave;nh danh s&aacute;ch c&aacute;c người chơi gh&eacute;p đ&uacute;ng 2 bức ảnh trong chương tr&igrave;nh sẽ tổng hợp v&agrave; so s&aacute;nh với 6 số cuối số điện thoại (6 SĐT) với d&atilde;y số ngẫu nhi&ecirc;n gồm 6 chữ số (từ 0 - 999999), th&ocirc;ng qua trang web www.random.org.&nbsp;<br />Chọn ra 126 người c&oacute; kết quả TR&Ugrave;NG hoặc NHỎ HƠN gần nhất với d&atilde;y số ngẫu nhi&ecirc;n:&nbsp;<br />- 6 Người may mắn đầu ti&ecirc;n sẽ tr&uacute;ng Điện thoại OPPO F1s.<br />- 20 người tiếp theo tr&uacute;ng 1 bộ gậy chụp ảnh 3 trong 1.<br />- 100 người c&oacute; kết quả nhỏ hơn tiếp theo với d&atilde;y số ngẫu nhi&ecirc;n sẽ nhận ngay 100 phiếu mua h&agrave;ng trị gi&aacute; 200.000đ. ( &Aacute;p dụng mua phụ kiện mới - c&aacute;c sản phẩm Oppo).&nbsp;</p>\r\n<p>Phiếu mua h&agrave;ng sẽ gửi qua điện thoại của kh&aacute;ch h&agrave;ng trước 17:00 ng&agrave;y h&ocirc;m sau, t&iacute;nh từ thời gian c&ocirc;ng bố danh s&aacute;ch tr&uacute;ng giải, trường hợp tr&uacute;ng giải v&agrave;o ng&agrave;y thứ 7, chủ nhật sẽ gửi trước 12 giờ ng&agrave;y thứ 2 tuần kế tiếp.<br />Kh&aacute;ch h&agrave;ng tr&uacute;ng giải điện thoại OPPO F1s - 1 bộ gậy chụp ảnh 3 trong 1&nbsp;sẽ được ch&uacute;ng t&ocirc;i li&ecirc;n lạc trong v&ograve;ng 48h kể từ ng&agrave;y c&ocirc;ng bố kết quả.</p>\r\n<p>Quay số v&agrave;o l&uacute;c 09:00 ng&agrave;y 01/10/2016.<br />C&ocirc;ng bố danh s&aacute;ch tr&uacute;ng gỉai l&uacute;c 16:00 01/10/2016.<br />Trường hợp d&atilde;y số may mắn c&oacute; 5,4,3&hellip; chữ số, sẽ so s&aacute;nh 5,4,3,&hellip; số cuối SĐT của kh&aacute;ch h&agrave;ng.<br />Thời gian nhận giải chung cuộc: từ 04/10 - 10/10.</p>', 1),
(35, 'Tin tức', 'Những thuê bao nào sẽ không được chuyển mạng giữ số?', '<p>Dự thảo th&ocirc;ng tư quy định về chuyển mạng giữ nguy&ecirc;n số của Bộ TT&amp;TT c&oacute; quy định những thu&ecirc; bao kh&ocirc;ng đăng k&yacute; th&ocirc;ng tin c&aacute; nh&acirc;n ch&iacute;nh x&aacute;c, thu&ecirc; bao nợ cước, thu&ecirc; bao vi phạm ph&aacute;p luật hoặc đang bị điều tra&hellip; sẽ kh&ocirc;ng được chuyển mạng giữ nguy&ecirc;n số.</p>', 'website/view/tintuc/20161021040016fa.jpg', '<p>Trong dự thảo th&ocirc;ng tư quy định về chuyển mạng giữ nguy&ecirc;n số của Bộ TT&amp;TT đang lấy &yacute; kiến nh&acirc;n d&acirc;n c&oacute; đưa ra những quy định về điều kiện cho thu&ecirc; bao chuyển mạng giữ nguy&ecirc;n số.</p>\r\n<p>Theo đ&oacute;, c&aacute;c mạng di động phải cung cấp dịch vụ chuyển mạng một c&aacute;ch b&igrave;nh đẳng, c&ocirc;ng bằng v&agrave; kh&ocirc;ng ph&acirc;n biệt đối cho tất cả c&aacute;c thu&ecirc; bao&nbsp;<a type="di động" href="https://www.thegioididong.com/dtdd-samsung" target="_blank">di động</a>. Thời gian ho&agrave;n th&agrave;nh qu&aacute; tr&igrave;nh chuyển mạng tối đa 7 ng&agrave;y l&agrave;m việc kể từ ng&agrave;y nhận được y&ecirc;u cầu chuyển mạng của thu&ecirc; bao đăng k&yacute; chuyển mạng đ&aacute;p ứng đủ c&aacute;c điều kiện.</p>\r\n<p>Dịch vụ th&ocirc;ng tin di động mặt đất cung cấp cho thu&ecirc; bao đăng k&yacute; dịch vụ chuyển mạng theo nguy&ecirc;n tắc Nh&agrave; mạng chuyển đi cắt dịch vụ sau đ&oacute; Nh&agrave; mạng chuyển đến mới mở dịch vụ. Khoảng thời gian giữa cắt v&agrave; mở dịch vụ kh&ocirc;ng qu&aacute; 6 giờ.</p>\r\n<p>Dự thảo th&ocirc;ng tư n&agrave;y cũng đưa ra quy định cước dịch vụ chuyển mạng được ph&acirc;n chia giữa Nh&agrave; mạng chuyển đi v&agrave; Nh&agrave; mạng chuyển đến tr&ecirc;n cơ sở b&ugrave; đắp chi ph&iacute; cung cấp dịch vụ chuyển mạng sau khi được sự chấp thuận của Cục Viễn th&ocirc;ng. C&aacute;c b&ecirc;n tham gia cung cấp dịch vụ chuyển mạng v&agrave; sử dụng cơ sở dữ liệu thu&ecirc; bao chuyển mạng phải đảm bảo b&iacute; mật th&ocirc;ng tin của thu&ecirc; bao đăng k&yacute; dịch vụ chuyển mạng.</p>\r\n<div class="imgwrap"><img src="https://cdn.tgdd.vn/Files/2016/10/21/902866/sim-2.jpg" alt="Những thu&ecirc; bao n&agrave;o sẽ kh&ocirc;ng được chuyển mạng giữ số?" /></div>\r\n<p>Ngo&agrave;i ra, c&ograve;n c&oacute; quy định đối với thu&ecirc; bao trả sau phải th&ocirc;ng tin thu&ecirc; bao phải đầy đủ, ch&iacute;nh x&aacute;c so với hợp đồng cung cấp v&agrave; sử dụng dịch vụ viễn th&ocirc;ng m&agrave; thu&ecirc; bao đ&atilde; k&yacute; với Nh&agrave; mạng chuyển đi. Đối với thu&ecirc; bao trả trước: th&ocirc;ng tin thu&ecirc; bao phải đầy đủ, ch&iacute;nh x&aacute;c theo c&aacute;c quy định về việc đăng k&yacute; th&ocirc;ng tin thu&ecirc; bao trả trước.</p>\r\n<p>Thu&ecirc; bao đ&atilde; được Nh&agrave; mạng chuyển đi k&iacute;ch hoạt dịch vụ th&ocirc;ng tin di động mặt đất &iacute;t nhất 90 ng&agrave;y trước thời điểm đăng k&yacute; sử dụng dịch vụ chuyển mạng. Thu&ecirc; bao trả sau kh&ocirc;ng nợ cước của kỳ thanh to&aacute;n cước gần nhất trước ng&agrave;y đăng k&yacute; dịch vụ chuyển mạng. Thu&ecirc; bao trả sau kh&ocirc;ng sử dụng dịch vụ chuyển v&ugrave;ng quốc tế trong th&aacute;ng liền trước hoặc trong th&aacute;ng đăng k&yacute; dịch vụ chuyển mạng.</p>\r\n<p>Thu&ecirc; bao kh&ocirc;ng vi phạm ph&aacute;p luật hoặc đang bị điều tra về việc vi phạm theo y&ecirc;u cầu của c&aacute;c cơ quan nh&agrave; nước c&oacute; thẩm quyền sẽ được tiến h&agrave;nh chuyển mạng giữ nguy&ecirc;n số.</p>\r\n<p>Như vậy, đối với cả thu&ecirc; bao trả trước v&agrave; trả sau nếu vi phạm c&aacute;c quy định tr&ecirc;n sẽ kh&ocirc;ng được chuyển mạng giữ nguy&ecirc;n số. B&ecirc;n cạnh đ&oacute;, khi thu&ecirc; bao vi phạm điều khoản tại hợp đồng hoặc điều kiện giao dịch chung về cung cấp v&agrave; sử dụng dịch vụ viễn th&ocirc;ng với Nh&agrave; mạng chuyển đi hoặc nhận được y&ecirc;u cầu hủy chuyển mạng của chủ thu&ecirc; bao từ Trung t&acirc;m chuyển mạng quốc gia sau khi đăng k&yacute; chuyển mạng sẽ kh&ocirc;ng được chuyển mạng giữ nguy&ecirc;n số.</p>\r\n<p>Trước đ&oacute;, ng&agrave;y 23/5/2016, Bộ trưởng Bộ TT&amp;TT Trương Minh Tuấn đ&atilde; k&yacute; quyết định sửa đổi Quyết định số 1178 (ng&agrave;y 23/9/2013) của Bộ trưởng TT&amp;TT về việc ph&ecirc; duyệt Đề &aacute;n chuyển mạng giữ nguy&ecirc;n số thu&ecirc; bao tại Việt Nam.</p>\r\n<p>Theo đ&oacute;, dịch vụ chuyển mạng th&ocirc;ng tin di động giữ nguy&ecirc;n số thu&ecirc; bao sẽ được triển khai theo lộ tr&igrave;nh mới. Cụ thể, thời gian kết nối thử nghiệm dịch vụ k&eacute;o d&agrave;i &iacute;t nhất 6 th&aacute;ng, ho&agrave;n th&agrave;nh trước ng&agrave;y 30/6/2017 v&agrave; thời gian cung cấp dịch vụ ch&iacute;nh thức trước ng&agrave;y 31/12/2017.</p>\r\n<p>Tại buổi l&agrave;m việc của Bộ trưởng Bộ TT&amp;TT Trương Minh Tuấn với Cục Viễn th&ocirc;ng v&agrave;o ng&agrave;y 11/5/2016, Cục trưởng Cục Viễn th&ocirc;ng Nguyễn Đức Trung cho biết, dịch vụ chuyển mạng giữ nguy&ecirc;n số l&agrave; dịch vụ của nh&agrave; mạng cho ph&eacute;p thu&ecirc; bao đang ở mạng n&agrave;y nếu thấy mạng kh&aacute;c c&oacute; nhiều dịch vụ hấp dẫn hơn, hoặc chất lượng dịch vụ, v&ugrave;ng phủ s&oacute;ng tốt hơn c&oacute; thể chuyển sang l&agrave;m thu&ecirc; bao mạng mới m&agrave; vẫn giữ nguy&ecirc;n số điện thoại của m&igrave;nh.</p>\r\n<p>Đ&acirc;y l&agrave; một c&ocirc;ng cụ hữu hiệu để th&uacute;c đẩy cạnh tranh được nhiều nước tr&ecirc;n thế giới triển khai. Đ&acirc;y cũng l&agrave; một trong những cam kết của Việt Nam khi gia nhập Hiệp định TPP.</p>', 0),
(36, 'Tin tức', 'Chảo lửa phân khúc 6 triệu tại VN sắp có sự tham gia của HTC?', '<p>Nhiều bạn rất th&iacute;ch One A9 bởi smartphone n&agrave;y c&oacute; thiết kế khung vỏ kim loại v&agrave; rất giống với iPhone 6, iPhone 6s. Tin vui l&agrave; mẫu smartphone One A9s với ngoại h&igrave;nh tương tự cũng sắp tiến tới thị trường Việt Nam v&agrave; n&oacute; c&oacute; gi&aacute; phải chăng hơn.</p>', 'website/view/tintuc/20161021040627one-a9-1.jpg', '<p>Theo nguồn tin m&agrave; m&igrave;nh c&oacute; được th&igrave; One A9s sẽ cập bến thị trường Việt Nam trong thời gian tới đ&acirc;y.</p>\r\n<p>Về cấu h&igrave;nh, One A9s c&oacute; m&agrave;n h&igrave;nh 5 inch độ ph&acirc;n giải HD, b&ecirc;n trong l&agrave; vi xử l&yacute; Helio P10, bộ nhớ RAM 2/3 GB tương ứng bộ nhớ ROM 16/32 GB. Mặt sau l&agrave; camera 13 MP khẩu độ F/2.2, trong khi m&aacute;y ảnh trước l&agrave; m&aacute;y ảnh 5 MP với khẩu độ F/2.8.</p>\r\n<div class="imgwrap"><img src="https://cdn2.tgdd.vn/Files/2016/10/21/903097/one-a9-3.jpg" alt="Chảo lửa ph&acirc;n kh&uacute;c 6 triệu tại VN sắp c&oacute; sự tham gia của HTC?" /></div>\r\n<p>One A9s vẫn hỗ trợ cảm biến v&acirc;n tay ở n&uacute;t Home, pin dung lượng 2.300 mAh v&agrave; chạy tr&ecirc;n nền tảng Android 6.0.&nbsp;</p>\r\n<p>Hiện tại, One A9 được b&aacute;n với gi&aacute; 9 triệu (đặt mua online giảm chỉ c&ograve;n hơn 6 triệu đồng) v&igrave; vậy dự kiến One A9s sẽ c&oacute; mức gi&aacute; cũng trong tầm 6-7 triệu đồng m&agrave; th&ocirc;i. Theo bạn th&igrave; mức gi&aacute; đ&oacute; c&oacute; hợp l&yacute;?</p>', 1),
(40, 'Tin tức', 'Vì sao Nokia từng thất bại trước Apple?', '<p>G&atilde; khổng lồ Phần Lan thất bại trước Apple kh&ocirc;ng phải v&igrave; iPhone, m&agrave; v&igrave; những bất ổn đến từ b&ecirc;n trong nội bộ của h&atilde;ng, theo lời cựu chủ tịch Nokia Jorma Ollila.</p>', 'website/view/tintuc/20161023021430fa.jpg', '<p>M&ugrave;a thu năm 2007, chủ tịch của Nokia khi đ&oacute; l&agrave; Jorma Ollila, người vừa từ chức CEO một năm trước, hỏi &yacute; kiến 12 vị điều h&agrave;nh đứng đầu c&ocirc;ng ty rằng "liệu iPhone mới của Apple c&oacute; l&agrave; mối đe dọa hay kh&ocirc;ng".</p>\r\n<p>L&uacute;c đ&oacute;, 10 nh&acirc;n sự cấp cao của Nokia cho rằng iPhone sẽ l&agrave; đối thủ đ&aacute;ng gờm. Ai cũng thấy r&otilde; điều đ&oacute;, nhưng đối với con t&agrave;u khổng lồ như Nokia bấy giờ, n&eacute; tr&aacute;nh tảng băng iPhone l&agrave; điều v&ocirc; c&ugrave;ng kh&oacute;.</p>\r\n<p>Trong cuốn tự truyện mang t&ecirc;n "Against All Odds: Leading Nokia form Near Catastrophe to Global Success", Jorma Ollila cũng thừa nhận điều n&agrave;y.</p>\r\n<p>Đ&oacute; l&agrave; c&acirc;u chuyện về Nokia v&agrave;o đầu những năm 90 khi h&atilde;ng gặp kh&oacute; khăn chồng chất do quyết định đầu tư sai lầm, cho đến khi Jorma Ollila l&ecirc;n nắm quyền v&agrave; l&aacute;i con t&agrave;u Nokia đến th&agrave;nh c&ocirc;ng. Cuốn s&aacute;ch cũng n&oacute;i về giai đoạn 2007 v&agrave; sự diệt vong sau n&agrave;y của Nokia.</p>\r\n<p>Năm 2004, l&atilde;nh đạo của Nokia đ&atilde; nhận ra tương lai của thiết bị di động l&agrave; điện thoại th&ocirc;ng minh v&agrave; yếu tố cạnh tranh chủ chốt khi đ&oacute; sẽ l&agrave; hệ điều h&agrave;nh. Bằng chứng l&agrave; chiếc di động được định nghĩa &ldquo;smartphone&rdquo; l&uacute;c bấy giờ: Nokia 9000 đ&atilde; ra đời với c&aacute;c t&iacute;nh năng mới như gửi email, lướt web.</p>\r\n<p>Tuy nhi&ecirc;n, Nokia đ&atilde; đ&uacute;ng đắn về sản phẩm, sai lầm về thời gian. H&atilde;ng tung ra Nokia 9000 v&agrave;o năm 1996, khi c&ocirc;ng nghệ mạng kh&ocirc;ng d&acirc;y chưa sẵn s&agrave;ng để sử dụng, kh&aacute;ch h&agrave;ng đ&atilde; kh&ocirc;ng thực sự cảm nhận được sự &ldquo;th&ocirc;ng minh&rdquo; của di động n&agrave;y.</p>\r\n<div class="imgwrap"><img src="https://cdn4.tgdd.vn/Files/2016/10/23/903643/1.jpg" alt="Nokia 9000 (phải) được ra mắt năm 1996" /></div>\r\n<div class="captionnews">Nokia 9000 (phải) được ra mắt năm 1996</div>\r\n<p>D&ugrave; đi trước, nội bộ Nokia lại chia rẽ. H&atilde;ng c&oacute; tận 2 nh&oacute;m nghi&ecirc;n cứu hệ điều h&agrave;nh cho di động. Một nh&oacute;m nghi&ecirc;n cứu ph&aacute;t triển Symbian, hệ điều h&agrave;nh cũ kĩ c&oacute; tr&ecirc;n hầu hết c&aacute;c smartphone Nokia. Nh&oacute;m c&ograve;n lại nghi&ecirc;n cứu MeeGo, hệ điều h&agrave;nh d&ugrave;ng cho c&aacute;c m&aacute;y t&iacute;nh bảng, netbook hay c&aacute;c hệ thống nh&uacute;ng.</p>\r\n<p>Sự tham lam trong chiến lược n&agrave;y đ&atilde; chia rẽ Nokia từ b&ecirc;n trong, đ&aacute;nh mất ưu thế trước một Apple với iOS v&agrave; đội ngũ đồng nhất của họ. &ldquo;Ch&uacute;ng t&ocirc;i hiểu vấn đề, nhưng s&acirc;u trong t&acirc;m tr&iacute; ch&uacute;ng t&ocirc;i lại kh&ocirc;ng thể chấp nhận điều đang diễn ra. Những dự &aacute;n lớn cứ thế được tiếp tục. Những buổi dự b&aacute;o doanh thu cứ diễn ra, trong khi điều ch&uacute;ng t&ocirc;i cần l&agrave;m l&agrave; tập trung v&agrave;o tương lai xa hơn&rdquo;, Jorma viết trong tự truyện.</p>\r\n<p>C&acirc;y b&uacute;t Justin Fox của Bloomberg nhận định, Nokia đ&atilde; kh&ocirc;ng c&oacute; được cố gắng đổi mới n&agrave;o trước những thay đổi tr&ecirc;n thị trường, khi iPhone v&agrave; c&aacute;c smartphone dần chiếm thị phần của h&atilde;ng.</p>\r\n<p>Những vị l&atilde;nh đạo đầu ti&ecirc;n của Nokia khi đ&oacute; đ&atilde; hơn 30 tuổi, gồm Jorma Ollila, Olli-Pekka Kallasvuo, Pekka Ala-Pietela, Matti Alahuhta v&agrave; Sari Baldauf.</p>\r\n<p>Họ đ&atilde; c&ugrave;ng nhau lớn l&ecirc;n ở Phần Lan v&agrave; l&atilde;nh đạo Nokia. Tuy được dẫn dắt bởi Jorma, nhưng từng người trong nh&oacute;m đều c&oacute; quyền đưa ra quyết định như nhau. Mọi vấn đề lớn Nokia gặp phải đều được giải quyết khi &ldquo;The Five&rdquo; họp mặt để b&agrave;n bạc.</p>\r\n<p>Jorma kể &ocirc;ng chưa từng gặp được đội ngũ l&atilde;nh đạo n&agrave;o như thế trong c&aacute;c c&ocirc;ng ty, tập đo&agrave;n kh&aacute;c. Những năm th&aacute;ng huy ho&agrave;ng của Nokia c&oacute; được đều do &ldquo;The Five&rdquo; đưa quyết định nhanh ch&oacute;ng nhưng thống nhất.</p>\r\n<div class="imgwrap"><img src="https://cdn2.tgdd.vn/Files/2016/10/23/903643/2.jpg" alt="Jorma Ollila từng l&agrave; CEO của h&atilde;ng Nokia từ năm 1994 đến 2006" /></div>\r\n<div class="captionnews">Jorma Ollila từng l&agrave; CEO của h&atilde;ng Nokia từ năm 1994 đến 2006</div>\r\n<p>Thế nhưng, &ldquo;The Five&rdquo; bắt đầu tan r&atilde; khi năm 2004, Sari Baldauf quyết định từ chức v&agrave; rời khỏi ban điều h&agrave;nh Nokia v&igrave; b&agrave; cảm thấy kh&ocirc;ng c&ograve;n mặn m&agrave; với c&ocirc;ng việc. Matti Alahuhta khi đ&oacute; được bổ nhiệm l&agrave;m Ph&oacute; chủ tịch điều h&agrave;nh của Nokia nhưng &ocirc;ng lại ki&ecirc;m chức CEO của h&atilde;ng thang m&aacute;y Kone.</p>\r\n<p>Kh&ocirc;ng l&acirc;u sau khi &ldquo;The Five&rdquo; mất 2 th&agrave;nh vi&ecirc;n v&agrave; Jorma từ chức CEO, Pekka Ala-Pietela cũng rời khỏi Nokia v&agrave; để Olli-Pekka Kallasvuo điều h&agrave;nh c&ocirc;ng ty từ năm 2006 đến 2010.</p>\r\n<p>Theo c&acirc;y b&uacute;t Justin Fox, c&oacute; thể điều hủy diệt Nokia kh&ocirc;ng phải iPhone, m&agrave; ch&iacute;nh l&agrave; &ldquo;The Five&rdquo;. Sự tan r&atilde; của đội ngũ l&atilde;nh đạo n&agrave;y đ&atilde; đặt dấu chấm hết cho số phận của Nokia. H&atilde;ng sau đ&oacute; được Stephen Elop l&atilde;nh đạo v&agrave;o 2010 nhưng tầm nh&igrave;n sai lầm của vị thuyền trưởng kh&ocirc;ng mang quốc tịch Phần Lan n&agrave;y đ&atilde; khiến di sản của Nokia trở th&agrave;nh m&acirc;y kh&oacute;i.</p>\r\n<p>Nền tảng Qt, hệ điều h&agrave;nh Meltemi hay MeeGo đều bị &ocirc;ng từ bỏ để chuyển sang Windows Phone của Microsoft. Kết quả l&agrave; điện thoại Windows Phone trong năm 2012 chỉ b&aacute;n được khoảng 10 triệu m&aacute;y, trong khi lượng&nbsp;<a type="smartphone" href="https://www.thegioididong.com/dtdd" target="_blank">smartphone</a>&nbsp;b&aacute;n ra to&agrave;n cầu c&ugrave;ng năm l&agrave; 169 triệu m&aacute;y, theo số liệu từ Gartner.</p>\r\n<p>Hiện tại, c&aacute;i t&ecirc;n Nokia vẫn xuất hiện tr&ecirc;n c&aacute;c trang b&aacute;o. H&atilde;ng di động n&agrave;y được cho sẽ trở lại với thiết bị Nokia D1C để tuy&ecirc;n chiến Samsung v&agrave; Apple. Như đo&aacute;n trước sự trở lại n&agrave;y, Jorma đ&atilde; viết cuối s&aacute;ch: &ldquo;Lịch sử đ&atilde; cho ta gợi &yacute;. Sau sự chuyển m&igrave;nh, Nokia c&oacute; thể sẽ t&igrave;m được những thị trường mới m&agrave; h&atilde;ng c&oacute; thể ph&aacute;t triển&rdquo;.</p>', 1);
INSERT INTO `baiviet` (`stt`, `loaitin`, `tieude`, `tomtat`, `hinhanh`, `noidung`, `trangthai`) VALUES
(41, 'Khuyến mãi', 'Điện thoại iPhone 7 32GB 18.790.000₫', '<p>3 KHUYẾN M&Atilde;I &Aacute;P DỤNG ĐẾN 30/11</p>\r\n<p><span><span>Cơ hội tr&uacute;ng xe Liberty khi mua iPhone ở khu vực H&agrave; Nội</span></span></p>\r\n<p><span><span><span>Cơ hội tr&uacute;ng Voucher du lịch Ch&acirc;u &Acirc;u&nbsp;</span><label class="infoend">(đến 30/11)</label></span></span></p>\r\n<p><span><span><span>Giảm ngay 5% khi Mua Laptop/ Tablet</span></span></span></p>\r\n<p><span><span><span><br /></span></span></span></p>', 'website/view/tintuc/20161110044719iphone-7-screencap_1280.0.0.jpg', '<div class="characteristics">\r\n<h2>Đặc điểm nổi bật của iPhone 7 32GB</h2>\r\n<div id="owl-detail" class="owl-carousel owl-detail owl-theme">\r\n<div class="owl-wrapper-outer autoHeight">\r\n<div class="owl-wrapper">\r\n<div class="owl-item">\r\n<div class="item"><img class="lazyOwl" src="https://cdn4.tgdd.vn/Products/Images/42/74110/Slider/iphone-7-slider01a.jpg" alt="" /></div>\r\n</div>\r\n</div>\r\n</div>\r\n<div class="owl-controls clickable">\r\n<div class="owl-pagination">\r\n<div class="owl-page active"><span>&nbsp;</span></div>\r\n<div class="owl-page"><span>&nbsp;</span></div>\r\n<div class="owl-page"><span>&nbsp;</span></div>\r\n<div class="owl-page"><span>&nbsp;</span></div>\r\n<div class="owl-page"><span>&nbsp;</span></div>\r\n<div class="owl-page"><span>&nbsp;</span></div>\r\n<div class="owl-page"><span>&nbsp;</span></div>\r\n</div>\r\n</div>\r\n</div>\r\n</div>\r\n<div class="boxArticle">\r\n<h2>Bộ đ&ocirc;i iPhone 7 v&agrave; iPhone 7 Plus đ&atilde; ch&iacute;nh thức được Apple giới thiệu với nhiều n&acirc;ng cấp về mặt cấu h&igrave;nh cũng như t&iacute;nh năng.</h2>\r\n<h3><strong>Thiết kế quen thuộc</strong></h3>\r\n<p>Tương tự như bộ đ&ocirc;i&nbsp;<a href="https://www.thegioididong.com/dtdd/iphone-6s" target="_blank">iPhone 6s</a>&nbsp;v&agrave; 6s Plus ra mắt năm ngo&aacute;i th&igrave; thiết kế của&nbsp;<strong>iPhone 7</strong>&nbsp;năm nay cũng kh&ocirc;ng c&oacute; nhiều sự thay đổi. Điểm dễ d&agrave;ng nhận thấy sự kh&aacute;c biệt nhất giữa iPhone 6s v&agrave; iPhone 7 ch&iacute;nh l&agrave; phần dải nhựa bắt s&oacute;ng đ&atilde; được đưa l&ecirc;n phần cạnh tr&ecirc;n thay v&igrave; cắt ngang mặt lưng như trước.</p>\r\n<p><a class="preventdefault hoverZoomLink" href="https://www.thegioididong.com/images/42/74110/iphone-711.jpg"><img class="lazy" src="https://cdn1.tgdd.vn/Products/Images/42/74110/iphone-711.jpg" alt="Thiết kế kh&aacute; tương đồng với iPhone 6s" /></a></p>\r\n<p><em>Thiết kế kh&aacute; tương đồng với iPhone 6s</em></p>\r\n<p>Ngo&agrave;i ra th&igrave; phần camera tr&ecirc;n iPhone 7 cũng lồi l&ecirc;n kh&aacute; nhiều v&agrave; c&oacute; k&iacute;ch thước lớn hơn hẳn so với c&aacute;c thế hệ iPhone trước. iPhone 7 cũng được bổ sung th&ecirc;m m&agrave;u mới l&agrave; m&agrave;u đen b&oacute;ng m&agrave; Apple gọi l&agrave;&nbsp;Jet Black tuy nhi&ecirc;n m&agrave;u mới n&agrave;y kh&aacute; bỏng bẩy v&agrave; dễ bị trầy xước nếu bạn kh&ocirc;ng sắm cho m&igrave;nh một chiếc ốp lưng trong qu&aacute; tr&igrave;nh sử dụng.</p>\r\n<p><a class="preventdefault hoverZoomLink" href="https://www.thegioididong.com/images/42/74110/iphone-710.jpg"><img class="lazy" src="https://cdn3.tgdd.vn/Products/Images/42/74110/iphone-710.jpg" alt="M&agrave;u Jet Black mới" /></a></p>\r\n<p><em>M&agrave;u Jet Black mới</em></p>\r\n<p><a class="preventdefault hoverZoomLink" href="https://www.thegioididong.com/images/42/74110/iphone-75.jpg"><img class="lazy" src="https://cdn.tgdd.vn/Products/Images/42/74110/iphone-75.jpg" alt="Tất cả c&aacute;c m&agrave;u sắc của iPhone 7" /></a></p>\r\n<p><em>Tất cả c&aacute;c m&agrave;u sắc của iPhone 7</em></p>\r\n<p>Ph&iacute;m Home vật l&yacute; tr&ecirc;n iPhone cũng đ&atilde; biến mất v&agrave; thay v&agrave;o đ&oacute; l&agrave; sự kết hợp với&nbsp;Taptic Engine mới, li&ecirc;n kết với 3D Touch để biến th&agrave;nh một n&uacute;t&nbsp;<span>Home&nbsp;</span>cảm ứng.</p>\r\n<p><a class="preventdefault hoverZoomLink" href="https://www.thegioididong.com/images/42/74110/iphone-76.jpg"><img class="lazy" src="https://cdn2.tgdd.vn/Products/Images/42/74110/iphone-76.jpg" alt="Ph&iacute;m Home cảm ứng lực" /></a></p>\r\n<p><em>Ph&iacute;m Home cảm ứng lực</em></p>\r\n<h3><strong>Lần đầu ti&ecirc;n iPhone c&oacute; thể chống nước</strong></h3>\r\n<p><a href="https://www.thegioididong.com/dtdd-apple-iphone" target="_blank">Apple&nbsp;</a>đ&atilde; lắng nghe mong muốn của người ti&ecirc;u d&ugrave;ng v&agrave; đưa ti&ecirc;u chuẩn chống nước IP67 l&ecirc;n bộ đ&ocirc;i iPhone mới của m&igrave;nh. Giờ đ&acirc;y chiếc iPhone 7 của bạn c&oacute; thể sống s&oacute;t dưới nước ở độ s&acirc;u 1m trong khoảng thời gian 30 ph&uacute;t m&agrave; kh&ocirc;ng bị hư hỏng g&igrave;.</p>\r\n<p><a class="preventdefault hoverZoomLink" href="https://www.thegioididong.com/images/42/74110/iphone-7-5.jpg"><img class="lazy" src="https://cdn4.tgdd.vn/Products/Images/42/74110/iphone-7-5.jpg" alt="iPhone 7 hỗ trợ chống nước ti&ecirc;u chuẩn IP67" /></a></p>\r\n<p><em>iPhone 7 hỗ trợ chống nước ti&ecirc;u chuẩn IP67</em></p>\r\n<p>Tuy nhi&ecirc;n Apple cũng kh&ocirc;ng khuyến c&aacute;o người d&ugrave;ng mang theo thiết bị đi biển hay thường xuy&ecirc;n nh&uacute;ng thiết bị xuống nước.</p>\r\n<h3><strong>M&agrave;n h&igrave;nh đẹp hơn</strong></h3>\r\n<p>Vẫn l&agrave; k&iacute;ch thước 4.7 inch v&agrave; vẫn c&oacute; c&ocirc;ng nghệ cảm ứng lực 3D Touch nhưng giờ đ&acirc;y m&agrave;n h&igrave;nh của iPhone 7 đ&atilde; hỗ trợ kh&ocirc;ng gian m&agrave;u 3P gi&uacute;p m&aacute;y c&oacute; độ phủ m&agrave;u rộng hơn 25% so với thế hệ iPhone cũ.</p>\r\n<p><a class="preventdefault hoverZoomLink" href="https://www.thegioididong.com/images/42/74110/iphone-712.jpg"><img class="lazy" src="https://cdn1.tgdd.vn/Products/Images/42/74110/iphone-712.jpg" alt="M&agrave;n h&igrave;nh hiển thị đẹp hơn" /></a></p>\r\n<p><em>M&agrave;n h&igrave;nh hiển thị đẹp hơn</em></p>\r\n<h3><strong>Hiệu năng h&agrave;ng đầu</strong></h3>\r\n<p>Con chip Apple A9 vẫn đang một trong những con chip c&oacute; hiệu năng tốt nhất tr&ecirc;n thị trường v&agrave; với iPhone 7 th&igrave; Apple đ&atilde; n&acirc;ng cấp l&ecirc;n thế hệ mới l&agrave; Apple A10 với tốc độ nhanh hơn 40% so với thế hệ cũ nhưng điện năng ti&ecirc;u thụ lại giảm 20%.</p>\r\n<p><a class="preventdefault hoverZoomLink" href="https://www.thegioididong.com/images/42/74110/iphone-78.jpg"><img class="lazy" src="https://cdn3.tgdd.vn/Products/Images/42/74110/iphone-78.jpg" alt="Con chip mới mạnh hơn, tiết kiệm năng lượng hơn" /></a></p>\r\n<p><em>Con chip mới mạnh hơn, tiết kiệm năng lượng hơn</em></p>\r\n<p>Ngo&agrave;i ra th&igrave; con chip Apple A10 cũng cho hiệu năng đồ họa tăng 50% so với Apple A9.</p>\r\n<h3><strong>Camera iPhone 7 được n&acirc;ng cấp mạnh mẽ</strong></h3>\r\n<p>Camera l&agrave; điểm được Apple n&acirc;ng cấp nhiều nhất tr&ecirc;n bộ đ&ocirc;i iPhone mới. iPhone 7 sở hữu camera ch&iacute;nh vẫn c&oacute; độ ph&acirc;n giải 12 MP nhưng khẩu độ đ&atilde; được tăng l&ecirc;n F/1.8 c&ugrave;ng khả năng chống rung quang học 3 trục OIS được t&iacute;ch hợp sẵn.</p>\r\n<p><a class="preventdefault hoverZoomLink" href="https://www.thegioididong.com/images/42/74110/iphone-79.jpg"><img class="lazy" src="https://cdn.tgdd.vn/Products/Images/42/74110/iphone-79.jpg" alt="Camera với ống k&iacute;nh 6 th&agrave;nh phần" /></a></p>\r\n<p><em>Camera với ống k&iacute;nh 6 th&agrave;nh phần</em></p>\r\n<p>M&aacute;y cũng được t&iacute;ch hợp sẵn con chip xử l&yacute; h&igrave;nh ảnh ri&ecirc;ng đến từ Apple với tốc độ nhanh hơn 60% thế hệ cũ trong khi điện năng lại tiết kiệm hơn 30%.</p>\r\n<p><a class="preventdefault hoverZoomLink" href="https://www.thegioididong.com/images/42/74110/iphone-71-2.jpg"><img class="lazy" src="https://cdn2.tgdd.vn/Products/Images/42/74110/iphone-71-2.jpg" alt="Một h&igrave;nh ảnh được chụp với điều kiện ban ng&agrave;y" /></a></p>\r\n<p><em>Một h&igrave;nh ảnh được chụp với điều kiện ban ng&agrave;y</em></p>\r\n<p>Một t&iacute;nh năng nổi bật nữa lần đầu ti&ecirc;n xuất hiện tr&ecirc;n smartphone l&agrave; khả năng quay video 4K 60 khung h&igrave;nh tr&ecirc;n gi&acirc;y tr&ecirc;n bộ đ&ocirc;i iPhone mới.</p>\r\n<p><a class="preventdefault hoverZoomLink" href="https://www.thegioididong.com/images/42/74110/iphone-72-2.jpg"><img class="lazy" src="https://cdn4.tgdd.vn/Products/Images/42/74110/iphone-72-2.jpg" alt="Hỉnh ảnh kh&aacute;c được chụp từ iPhone 7" /></a></p>\r\n<p><em>Hỉnh ảnh kh&aacute;c được chụp từ iPhone 7</em></p>\r\n<h3><strong>Jack cắm tai nghe đ&atilde; kh&ocirc;ng c&ograve;n</strong></h3>\r\n<p>Apple đ&atilde; loại bỏ đi jack cắm&nbsp;<a href="https://www.thegioididong.com/tai-nghe" target="_blank">tai nghe</a>&nbsp;truyền thống tr&ecirc;n bộ đ&ocirc;i iPhone mới v&agrave; thay v&agrave;o đ&oacute; l&agrave; iPhone 7 sẽ sở hữu cho m&igrave;nh cặp loa k&eacute;p tr&ecirc;n dưới hứa hẹn sẽ cho chất lượng &acirc;m thanh tốt hơn.</p>\r\n<p><a class="preventdefault hoverZoomLink" href="https://www.thegioididong.com/images/42/74110/iphone-74-1.jpg"><img class="lazy" src="https://cdn1.tgdd.vn/Products/Images/42/74110/iphone-74-1.jpg" alt="Jack cắm tai nghe đ&atilde; bị loại bỏ" /></a></p>\r\n<p><em>Jack cắm tai nghe đ&atilde; bị loại bỏ</em></p>\r\n<p><a class="preventdefault hoverZoomLink" href="https://www.thegioididong.com/images/42/74110/iphone-77.jpg"><img class="lazy" src="https://cdn3.tgdd.vn/Products/Images/42/74110/iphone-77.jpg" alt="Trải nghiệm &acirc;m thanh hay hơn" /></a></p>\r\n<p><em>Trải nghiệm &acirc;m thanh hay hơn</em></p>\r\n<p>Apple iPhone 7 thực sự l&agrave;&nbsp;<a href="https://www.thegioididong.com/dtdd" target="_blank">smartphone&nbsp;</a>đ&aacute;ng sở hữu nhất tr&ecirc;n thị trường v&agrave;o dịp mua sắm cuối năm.</p>\r\n</div>', 1),
(42, 'Khuyến mãi', 'Hot Sale linh phụ kiện', '<p class="date"><strong>Từ ng&agrave;y 10/11 - 30/11/2016</strong></p>\r\n<h4><strong>Hot Sale linh phụ kiện</strong></h4>\r\n<p><strong><br /></strong></p>', 'website/view/tintuc/20161110045209asus.jpg', '<p>&nbsp;</p>\r\n<h2 class="crazy-thele-title">THỂ LỆ CHƯƠNG TR&Igrave;NH</h2>\r\n<div class="crazy-thele-scroll scroll ps-container ps-theme-default ps-active-y">\r\n<p><span>Thể lệ chương tr&igrave;nh HOT SALE</span></p>\r\n<p>- Với chương tr&igrave;nh&nbsp;<span>HOT SALE</span>&nbsp;, qu&yacute; kh&aacute;ch c&oacute; thể đặt mua h&agrave;ng loạt c&aacute;c sản phẩm linh phụ kiện hot nhất hiện nay với gi&aacute; th&agrave;nh giảm cực sốc. Chương tr&igrave;nh diễn ra v&agrave;o c&aacute;c ng&agrave;y thứ Năm v&agrave; thứ S&aacute;u h&agrave;ng tuần.</p>\r\n<p>- V&agrave;o ng&agrave;y tiếp theo sau khi kết th&uacute;c deal, tổng đ&agrave;i vi&ecirc;n &nbsp;sẽ li&ecirc;n hệ với kh&aacute;ch h&agrave;ng may mắn để x&aacute;c nhận đơn h&agrave;ng.</p>\r\n<p>- Sau 24h kể từ khi tổng đ&agrave;i vi&ecirc;n x&aacute;c nhận đơn h&agrave;ng, nếu qu&yacute; kh&aacute;ch từ chối nhận sản phẩm th&igrave; sản phẩm sẽ được giao cho kh&aacute;ch h&agrave;ng kế tiếp trong danh s&aacute;ch &ldquo;Chờ mua&rdquo;.</p>\r\n<p><span>*Lưu &yacute;:</span></p>\r\n<p>- Tất cả sản phẩm tham gia chương tr&igrave;nh đều l&agrave; sản phẩm mới, được đ&oacute;ng th&ugrave;ng (nguy&ecirc;n đai, nguy&ecirc;n kiện). Kh&aacute;ch h&agrave;ng vui l&ograve;ng kiểm tra kỹ sản phẩm trước khi nhận.</p>\r\n<p>- Thời gian mở deal v&agrave;o ng&agrave;y thứ Năm v&agrave; thứ S&aacute;u h&agrave;ng tuần, đến hết ng&agrave;y 30/11/2016.</p>\r\n<p>- Gi&aacute; b&aacute;n đ&atilde; bao gồm 10% VAT.</p>\r\n<p>- Chỉ &aacute;p dụng cho h&igrave;nh thức mua online v&agrave; giao h&agrave;ng tận nơi.</p>\r\n</div>', 1),
(43, 'Khuyến mãi', 'Còn gì bằng khi đã được Vina khuyến mãi 50% lại được Tiki giảm thêm đến 7% nạp thẻ online', '<p>Duy nhất v&agrave;o ng&agrave;y 23.09.2016 tại Tiki.vn, kh&aacute;ch h&agrave;ng sẽ được giảm th&ecirc;m đến 7% khi nạp thẻ online đồng thời nhận khuyến m&atilde;i 50% từ nh&agrave; mạng Vina.</p>', 'website/view/tintuc/2016111808251620161110050431asus (1).jpg', '<p>Dịch vụ nạp thẻ online của Tiki.vn mới chỉ được cho ra mắt từ th&aacute;ng 8/2016 nhưng chỉ sau 1 th&aacute;ng, n&oacute; đ&atilde; thu h&uacute;t h&agrave;ng trăm ngh&igrave;n người tham gia giao dịch. L&yacute; do l&agrave; bởi thay v&igrave; phải lặn lội đi mua thẻ, dịch vụ nạp thẻ online của Tiki sẽ gi&uacute;p kh&aacute;ch h&agrave;ng kh&ocirc;ng phải đi ra ngo&agrave;i để mua thẻ m&agrave; chỉ cần ngồi nh&agrave; v&agrave; nhấn n&uacute;t l&agrave; ngay lập tức tiền sẽ &ldquo;chảy&rdquo; v&agrave;o t&agrave;i khoản điện thoại nhanh ch&oacute;ng.</p>\r\n<p>Kh&ocirc;ng những vậy, ngo&agrave;i ưu đ&atilde;i khuyến mại từ nh&agrave; mạng, Tiki.vn c&ograve;n giảm th&ecirc;m đến 7% khi nạp thẻ cho kh&aacute;ch h&agrave;ng, đồng thời miễn ph&iacute; giao dịch khi nạp thẻ.</p>\r\n<p class="VCSortableInPreviewModeWrapper">&nbsp;</p>\r\n<p><em>Để tham gia nạp thẻ online tiện &iacute;ch, kh&aacute;ch h&agrave;ng chỉ cần click chuột đơn giản&nbsp;<a rel="nofollow" href="https://tiki.vn/dich-vu-tien-ich?utm_source=genk&amp;utm_medium=pr&amp;utm_campaign=genk_VA.VINAKM230916" target="_blank">tại đ&acirc;y</a>.</em></p>\r\n<p>Đối với kh&aacute;ch h&agrave;ng khi tham gia dịch vụ tiện &iacute;ch nạp thẻ online tại Tiki.vn trong ng&agrave;y 23.09.16 sẽ nhận được mọi ưu đ&atilde;i hiện c&oacute; bao gồm: giảm th&ecirc;m đến 7% khi nạp thẻ đồng thời nhận khuyến m&atilde;i 50% từ nh&agrave; mạng Vina (Tặng th&ecirc;m 100% ng&agrave;y sử dụng của thẻ cho thu&ecirc; bao VinaCard, Vina365). Gi&aacute; trị thẻ nạp c&oacute; thể sử dụng để nạp ngay v&agrave;o số điện thoại hoặc lưu trữ trong SMS hoặc t&agrave;i khoản h&ograve;m thư c&aacute; nh&acirc;n của kh&aacute;ch h&agrave;ng cho tới thời điểm cần sử dụng. Đặc biệt,kh&aacute;ch h&agrave;ng sử dụng thu&ecirc; bao trả sau vẫn c&oacute; thể mua thẻ nạp thanh to&aacute;n cước b&igrave;nh thường như thu&ecirc; bao trả trước.</p>\r\n<p>Cụ thể, đối với những thẻ mệnh gi&aacute; 100.000đ chỉ cần thanh to&aacute;n 97.000đ, thẻ 200.000đ chỉ c&ograve;n 194.000đ, thẻ 300.000đ chỉ c&ograve;n 291.000đ v&agrave; đặc biệt thẻ mệnh gi&aacute; 500.000đ kh&aacute;ch hang sẽ chỉ phải thanh to&aacute;n 475.000đ đồng thời miễn ph&iacute; to&agrave;n bộ giao dịch khi nạp thẻ.</p>\r\n<p class="VCSortableInPreviewModeWrapper">&nbsp;</p>\r\n<div class="VCSortableInPreviewMode">\r\n<div><img src="http://channel.vcmedia.vn/thumb_w/640/prupload/441/2016/09/img20160922160038142.jpg" alt="" /></div>\r\n</div>\r\n<p>&nbsp;</p>\r\n<p>Qu&yacute; kh&aacute;ch h&agrave;ng h&atilde;y nhanh tay nạp tiền điện thoại qua&nbsp;<a class="link-inline-content" rel="nofollow" href="https://tiki.vn/dich-vu-tien-ich?utm_source=genk&amp;utm_medium=pr&amp;utm_campaign=genk_VA.VINAKM230916" target="_blank">Tiki.vn</a>&nbsp;để trải nghiệm dịch vụ nạp tiền v&ocirc; c&ugrave;ng tiện lợi, kh&ocirc;ng lo phải đi t&igrave;m mua thẻ nạp lại nhận ngay chiết khấu tới 57%. Tiki.vn mong muốn sẽ tiếp tục mang đến cho kh&aacute;ch h&agrave;ng những trải nghiệm mua sắm tuyệt vời nhất, kh&ocirc;ng chỉ đơn giản l&agrave; mua sắm m&agrave; đ&oacute; c&ograve;n l&agrave; những ph&uacute;t gi&acirc;y thư gi&atilde;n v&agrave; h&agrave;i l&ograve;ng với dịch vụ mua sắm trực tuyến mỗi ng&agrave;y.</p>', 1),
(44, 'Khuyến mãi', 'Microsoft tặng Xbox One cho sinh viên và học sinh khi mua Surface Pro 4', '<p>Đ&acirc;y l&agrave; chương tr&igrave;nh khuyến m&atilde;i lớn của Microsoft cho c&aacute;c bạn học sinh v&agrave; sinh vi&ecirc;n.</p>', 'website/view/tintuc/20161110050811asus.jpg', '<p><span>C&oacute; lẽ đ&acirc;y l&agrave; chương tr&igrave;nh khuyến m&atilde;i đặc biệt nhất m&agrave;&nbsp;<a title="Microsoft" href="http://genk.vn/microsoft.htm" target="_blank">Microsoft</a>&nbsp;d&agrave;nh tặng cho c&aacute;c bạn sinh vi&ecirc;n v&agrave; học sinh, được &aacute;p dụng tại c&aacute;c cửa h&agrave;ng b&aacute;n lẻ của Microsoft tại Mỹ. Theo đ&oacute;, c&aacute;c bạn học sinh v&agrave; sinh vi&ecirc;n khi mua Surface Pro 4 c&ugrave;ng với cả Xbox One sẽ được tặng ng&agrave;y 300 USD.</span></p>\r\n<p>Đ&oacute; cũng l&agrave; gi&aacute; b&aacute;n của chiếc m&aacute;y chơi game Xbox One, v&igrave; vậy cũng đồng nghĩa với việc Microsoft sẽ tặng Xbox One khi mua Surface Pro 4. Chương tr&igrave;nh khuyến m&atilde;i n&agrave;y sẽ k&eacute;o d&agrave;i từ nay đến hết ng&agrave;y 14 th&aacute;ng 8.</p>\r\n<p>Thời điểm Microsoft tung ra chương tr&igrave;nh khuyến m&atilde;i đặc biệt n&agrave;y, cũng l&agrave; sau khi tuy&ecirc;n bố sẽ ngừng sản xuất Surface 3. Đ&acirc;y l&agrave; chiếc tablet hướng tới đối tượng sinh vi&ecirc;n v&agrave; học sinh, với gi&aacute; rẻ hơn so với c&aacute;c phi&ecirc;n bản Surface Pro.</p>\r\n<p>Với việc khai tử Surface 3, Microsoft được ho l&agrave; sẽ tập trung v&agrave;o d&ograve;ng sản phẩm cao cấp hơn l&agrave; Surface Pro v&agrave; Surface Book. Hiện tại cũng chưa c&oacute; th&ocirc;ng tin g&igrave; về phi&ecirc;n bản kế nhiệm của Surface 3.</p>\r\n<p>B&ecirc;n cạnh đ&oacute;, Microsoft cũng tiết lộ kế hoạch ra mắt phi&ecirc;n bản Xbox One S mới v&agrave;o cuối năm nay. Đ&acirc;y l&agrave; chiếc Xbox One nhỏ gọn hơn v&agrave; cũng mạnh hơn so với phi&ecirc;n bản trước.</p>\r\n<p>C&oacute; lẽ v&igrave; thế m&agrave; Microsoft muốn dọn kho h&agrave;ng của m&igrave;nh, để chuẩn bị cho c&aacute;c sản phẩm mới ra mắt</p>\r\n<p>&nbsp;</p>', 1),
(45, 'Khuyến mãi', 'Mua Sony Xperia XZ tặng Photobook hình ảnh chính bạn trị giá 1,25 triệu', '', 'website/view/tintuc/20161110051147asus.jpg', '<p><strong>1. T&ecirc;n chương tr&igrave;nh:&nbsp;</strong>Sony Xperia XZ chụp h&igrave;nh cực đẹp</p>\r\n<p><strong>2. Thời gian&nbsp;</strong>từ 04/11/2016 đến 31/12/2016</p>\r\n<p><strong>3. Sản phẩm &aacute;p dụng:&nbsp;</strong>Xperia XZ (F8332)</p>\r\n<p><strong>4. Qu&agrave; tặng:</strong>&nbsp;1&nbsp;Photobook c&aacute; nh&acirc;n, trị gi&aacute; 1,250,000 VND/pcs, gồm 40 trang c&oacute; k&iacute;ch thước nhỏ 8 inch x 6 inch (20cm x 15 cm) + ph&iacute; chuyển ph&aacute;t nhanh.</p>\r\n<p><strong>5. Nội dung chương tr&igrave;nh:&nbsp;</strong></p>\r\n<p>Trong thời gian khuyến m&atilde;i, kh&aacute;ch h&agrave;ng mua Xperia XZ tại hệ thống si&ecirc;u thị Thegioididong v&agrave; Dienmayxanh sẽ nhận được&nbsp;<strong>1 Code khuyến m&atilde;i</strong>&nbsp;th&ocirc;ng qua ứng dụng Xperia Lounge được c&agrave;i đặt trong m&aacute;y XZ</p>\r\n<p>Kh&aacute;ch h&agrave;ng sẽ truy cập v&agrave;o website của chương tr&igrave;nh<a title="a" rel="nofollow" href="http://www.photobookvietnam.com/campaign/sony_vn"><strong>http://www.photobookvietnam.com/campaign/sony_vn</strong></a>&nbsp;rồi upload h&igrave;nh chụp từ điện thoại XZ, c&aacute; nh&acirc;n ho&aacute; những ảnh của kh&aacute;ch h&agrave;ng với sự hướng dẫn chi tiết r&otilde; r&agrave;ng tr&ecirc;n trang web v&agrave; nhập m&atilde; khuyến mại để ho&agrave;n chỉnh bức ảnh cho Photobook.</p>\r\n<p>Photobook của kh&aacute;ch h&agrave;ng sẽ được tạo v&agrave; fee ship đến nh&agrave; kh&aacute;ch h&agrave;ng qua dịch vụ chuyển ph&aacute;t nhanh DHL từ 7 &ndash; 10 ng&agrave;y.</p>\r\n<p><strong>6. Hướng dẫn:</strong></p>\r\n<p>Bước 1:&nbsp;truy cập v&agrave;o website của chương tr&igrave;nh http://www.photobookvietnam.com/campaign/sony_vn</p>\r\n<p>Bấm v&agrave;o n&uacute;t "Bắt đầu thực hiện s&aacute;ch ảnh của bạn"</p>\r\n<div class="imgwrap"><img title="sony" src="https://cdn1.tgdd.vn/Files/2016/11/01/907771/1_427x151.jpg" alt="a" /></div>\r\n<p>&nbsp;</p>\r\n<p>Đăng nhập t&agrave;i khoản photobook. Nếu chưa c&oacute; t&agrave;i khoản bấm n&uacute;t đăng k&yacute;.</p>\r\n<div class="imgwrap"><img title="sony" src="https://cdn4.tgdd.vn/Files/2016/11/01/907771/2_616x397.jpg" alt="a" /></div>\r\n<p>Bước 2: Bấm n&uacute;t upload h&igrave;nh ảnh chụp từ m&aacute;y Sony Xperia XZ</p>\r\n<p>Sử dụng c&aacute;c c&ocirc;ng cụ c&oacute; sẵn để thiết kế bộ Photobook của bạn</p>\r\n<p>Ho&agrave;n tất thiết kế.</p>\r\n<div class="imgwrap"><img title="photo" src="https://cdn3.tgdd.vn/Files/2016/11/01/907771/sony2_800x424.jpg" alt="a" /></div>\r\n<p>Bước 3: Cập nhật địa chỉ nhận h&agrave;ng hiện tại của bạn. Lựa chọn phương thức giao h&agrave;ng.</p>\r\n<p>Lưu &yacute;: Phương thức giao h&agrave;ng ti&ecirc;u chuẩn mất 24 ng&agrave;y l&agrave;m việc phụ thuộc v&agrave;o địa điểm giao h&agrave;ng.</p>\r\n<p>Nhập m&atilde;&nbsp;<strong>Code khuyến m&atilde;i</strong>&nbsp;nhận được từ ứng dụng Xperia Lounge v&agrave; chọn &ldquo;Quy đổi&rdquo; (Redeem) để bắt đầu đặt s&aacute;ch ảnh.&nbsp;</p>\r\n<p>Bước 4: kiểm tra lại to&agrave;n bộ th&ocirc;ng tin trước khi đặt s&aacute;ch ảnh.</p>\r\n<h2>Li&ecirc;n hệ contact hotline&nbsp;<strong>08 9854 0734 v&agrave; e-mail support@photobook.sony.com.vn</strong>sẽ hỗ trợ trong việc tạo photobook.</h2>\r\n<p><strong>7. Quy định:&nbsp;</strong></p>\r\n<p>+ M&atilde; qu&agrave; tặng chỉ được &aacute;p dụng cho s&aacute;ch ảnh gồm 40 trang c&oacute; k&iacute;ch thước nhỏ 8 inch x 6 inch (20cm x 15 cm) bao gồm ph&iacute; chuyển ph&aacute;t nhanh.</p>\r\n<p>+ M&atilde; qu&agrave; tặng c&oacute; hiệu lực đến ng&agrave;y 31/3/2017</p>\r\n<p>+ Chương tr&igrave;nh chỉ &aacute;p dụng cho vận chuyển trong địa phận Việt Nam.&nbsp;</p>\r\n<p>+ Chi ph&iacute; ph&aacute;t sinh sẽ được t&iacute;nh nếu n&acirc;ng cấp v&agrave; th&ecirc;m trang ảnh.</p>\r\n<p>+ Kh&ocirc;ng bao gồm h&igrave;nh thức tự nhận h&agrave;ng v&agrave; gộp vận chuyển.</p>\r\n<p>+ Mỗi m&atilde; qu&agrave; tặng chỉ được sử dụng một (01) lần.</p>\r\n<p>+ Kh&ocirc;ng &aacute;p dụng giao h&agrave;ng chung với c&aacute;c sản phẩm kh&aacute;c.</p>\r\n<p>+ Kh&ocirc;ng &aacute;p dụng đồng thời với c&aacute;c chương tr&igrave;nh khuyến m&atilde;i kh&aacute;c.</p>\r\n<p>+ Nhập m&atilde; qu&agrave; tặng để được tặng miễn ph&iacute; khi đăng k&yacute; nhận s&aacute;ch ảnh.</p>\r\n<p>+ M&atilde; qu&agrave; tặng sẽ kh&ocirc;ng c&ograve;n hiệu lực sau khi ho&agrave;n tất đăng k&yacute; nhận s&aacute;ch ảnh.</p>\r\n<p>+ Qu&agrave; tặng kh&ocirc;ng được chuyển qua giảm gi&aacute; sản phẩm.</p>\r\n<p>+ M&atilde; qu&agrave; tặng chỉ c&oacute; gi&aacute; trị hiện vật, kh&ocirc;ng d&ugrave;ng để thanh to&aacute;n v&agrave; cũng kh&ocirc;ng &aacute;p dụng c&aacute;c khoản thuế hoặc ph&iacute; vận chuyển.</p>\r\n<p>+ Mỗi ho&aacute; đơn mua Xperia XZ sẽ được nhận một (1) m&atilde; qu&agrave; tặng tr&ecirc;n ứng dụng Xperia Lounge của điện thoại Xperia XZ của bạn.</p>\r\n<p>+ M&atilde; qu&agrave; tặng sẽ được nhận trực tuyến tr&ecirc;n ứng dụng Xperia Lounge của điện thoại Xperia XZ của bạn.</p>\r\n<p>+ M&atilde; qu&agrave; tặng kh&ocirc;ng c&oacute; gi&aacute; trị hiện kim, kh&ocirc;ng d&ugrave;ng để b&aacute;n v&agrave; kh&ocirc;ng thể quy đổi ra tiền mặt.</p>\r\n<p>+ Kh&ocirc;ng chấp nhận thay thế hoặc chuyển đổi m&atilde; qu&agrave; tặng</p>\r\n<p>+ Ch&uacute;ng t&ocirc;i sẽ kh&ocirc;ng chịu tr&aacute;ch nhiệm với bất k&igrave; tổn thất hay thiệt hại n&agrave;o li&ecirc;n quan đến:</p>\r\n<p>a. Giao dịch mua, quy đổi hay sử dụng m&atilde; qu&agrave; tặng của bạn.</p>\r\n<p>b. Trường hợp bạn đ&aacute;nh mất hoặc qu&ecirc;n m&atilde; qu&agrave; tặng. Trường hợp m&atilde; qu&agrave; tặng bị v&ocirc; hiệu hoặc kh&ocirc;ng nhận được v&igrave; cung cấp sai th&ocirc;ng tin li&ecirc;n hệ.</p>\r\n<p>c. Trường hợp người kh&aacute;c sử dụng m&atilde; qu&agrave; tặng của bạn khi chưa được sự x&aacute;c nhận hoặc đồng &yacute;.</p>\r\n<p>+ Vui l&ograve;ng xem qua &ldquo;Ch&iacute;nh s&aacute;ch đổi h&agrave;ng&rdquo; của ch&uacute;ng t&ocirc;i, Photobook Worldwide sẽ kh&ocirc;ng chịu tr&aacute;ch nhiệm đối với trường hợp lỗi do kh&aacute;ch h&agrave;ng sử dụng. Nếu lỗi do nh&agrave; sản xuất,ch&uacute;ng t&ocirc;i sẽ đ&aacute;nh gi&aacute; t&igrave;nh trạng lỗi của s&aacute;ch ảnh để quyết định sửa chữa hay đổi mới.</p>\r\n<p>+ Kh&ocirc;ng &aacute;p dụng ho&agrave;n tiền đối với s&aacute;ch ảnh sử dụng m&atilde; qu&agrave; tặng.</p>\r\n<p>+ Đối với s&aacute;ch ảnh đ&atilde; ho&agrave;n th&agrave;nh đăng k&yacute; vận chuyện, bạn sẽ kh&ocirc;ng thể thay đổi hay chỉnh sửa th&ecirc;m. V&igrave; vậy, bạn c&oacute; tr&aacute;ch nhiệm kiểm tra kĩ s&aacute;ch ảnh của m&igrave;nh trước khi tiến h&agrave;nh đăng k&yacute; vận chuyển.</p>\r\n<p>+ Photobook c&oacute; quyền tự thay đổi điều khoản v&agrave; điều kiện. Ch&uacute;ng t&ocirc;i c&oacute; quyền chấm dứt chương tr&igrave;nh khuyến m&atilde;i bất k&igrave; l&uacute;c n&agrave;o.</p>\r\n<p>Mọi thắc mắc về chương tr&igrave;nh n&agrave;y qu&yacute; kh&aacute;ch li&ecirc;n hệ&nbsp;<strong>hotline 08 9854 0734</strong>&nbsp;v&agrave;&nbsp;<strong>e-mail support@photobook.sony.com.vn</strong>&nbsp;sẽ hỗ trợ trong việc tạo photobook.</p>\r\n<p>Tr&acirc;n trọng.</p>', 1),
(46, 'Khuyến mãi', 'Mua Apple – Trúng 7 Voucher du lịch Châu Âu trong tháng 11 này!', '<p>D&agrave;nh ri&ecirc;ng cho fan của "T&aacute;o" trong th&aacute;ng 11 n&agrave;y, Thế giới di động xin giới thiệu một chương tr&igrave;nh thật hấp dẫn. Mua sản phẩm của Apple tr&uacute;ng 7 Voucher du lịch Ch&acirc;u &Acirc;u.</p>', 'website/view/tintuc/20161110051446tải xuống.png', '<p><strong><span style="text-decoration: underline;">ĐỐI TƯỢNG KH&Aacute;CH H&Agrave;NG</span></strong></p>\r\n<p>Chương tr&igrave;nh d&agrave;nh ri&ecirc;ng cho tất cả kh&aacute;ch h&agrave;ng đặt mua Điện thoại, M&aacute;y t&iacute;nh bảng, Laptop thương hiệu Apple&nbsp;(Online hoặc tại trực tiếp hệ thống si&ecirc;u thị Thegioididong.com &amp; DienmayXanh.com).</p>\r\n<p><strong><span style="text-decoration: underline;">THỜI GIAN DIỄN RA</span></strong><span style="text-decoration: underline;">:&nbsp;</span>từ&nbsp;<strong>01/11 &ndash; 30/11/2016</strong></p>\r\n<p><strong><span style="text-decoration: underline;">TỔNG GIẢI THƯỞNG:</span></strong>&nbsp;<strong>7</strong>&nbsp;Voucher du lịch Ch&acirc;u &Acirc;u của Viettravel (mỗi Voucher trị gi&aacute; 70.000.000đ)</p>\r\n<ul>\r\n<li>Giải thưởng kh&ocirc;ng được quy đổi th&agrave;nh tiền mặt.</li>\r\n<li>Được quyền chuyển nhượng lại cho người kh&aacute;c theo y&ecirc;u của kh&aacute;ch h&agrave;ng.</li>\r\n<li>Voucher c&oacute; gi&aacute; trị đến hết ng&agrave;y&nbsp;<strong>07</strong><strong>/</strong><strong>04/2017</strong></li>\r\n<li>Nếu kh&aacute;ch h&agrave;ng đi tour c&oacute; gi&aacute; trị cao hơn sẽ b&ugrave; ph&iacute; ch&ecirc;nh lệch.</li>\r\n<li>C&ocirc;ng ty cam kết gi&aacute; trị giải thưởng tr&ecirc;n l&agrave; ch&iacute;nh x&aacute;c, được x&aacute;c định tr&ecirc;n cơ sở b&aacute;o gi&aacute; v&agrave; h&oacute;a đơn đầu v&agrave;o cho việc mua v&agrave; sản xuất c&aacute;c giải thưởng của c&ocirc;ng ty.</li>\r\n</ul>\r\n<p><strong><span style="text-decoration: underline;">CƠ CẤU CHỌN KH&Aacute;CH H&Agrave;NG TR&Uacute;NG GIẢI</span></strong></p>\r\n<p>Ch&uacute;ng t&ocirc;i sẽ lựa chọn ra 7 kh&aacute;ch h&agrave;ng may mắn&nbsp;nhất để trao tặng Voucher, theo nguy&ecirc;n tắc tập hợp m&atilde; dự thưởng của c&aacute;c kh&aacute;ch h&agrave;ng tham gia chương tr&igrave;nh theo từng mốc thời gian cụ thể v&agrave;&nbsp;d&ugrave;ng&nbsp;hệ thống lựa chọn ngẫu nhi&ecirc;n (phần mềm quay số điện tử) của Thế Giới Di Động&nbsp;để chọn ra 7 m&atilde; dự thưởng may mắn nhất (mỗi m&atilde; dự thưởng ứng với 1 kh&aacute;ch h&agrave;ng).&nbsp;M&atilde; dự thưởng bao gồm k&yacute; hiệu (phần chữ) v&agrave; số ho&aacute; đơn (phần số).</p>\r\n<p><em>V&iacute; dụ: Ho&aacute; đơn mua h&agrave;ng của kh&aacute;ch h&agrave;ng c&oacute; k&yacute; hiệu l&agrave;&nbsp;<strong>HP/15E</strong>&nbsp;v&agrave; số ho&aacute; đơn l&agrave;&nbsp;<strong>0027616</strong>&nbsp;th&igrave; m&atilde; số dự thưởng của kh&aacute;ch h&agrave;ng đ&oacute; l&agrave;&nbsp;<strong>HP/15E0027616</strong>.</em></p>\r\n<p><strong>(*) LƯU &Yacute;: Kh&aacute;ch h&agrave;ng sau khi mua h&agrave;ng vui l&ograve;ng giữ lại ho&aacute; đơn mua h&agrave;ng.&nbsp;</strong></p>\r\n<p><strong>Cụ thể c&aacute;c mốc thời gian lựa chọn kh&aacute;ch h&agrave;ng may mắn như sau:</strong></p>\r\n<ul>\r\n<li>Đợt 1. Gồm c&aacute;c m&atilde; dự thưởng từ ng&agrave;y&nbsp;<strong>01/11/2016</strong>&nbsp;đến&nbsp;<strong>11/11/2016.&nbsp;</strong>Quay số ng&agrave;y<strong>12/11/2016</strong>, chọn ra 2 kh&aacute;ch h&agrave;ng tr&uacute;ng thưởng, c&ocirc;ng bố kết quả trước 20:00 c&ugrave;ng ng&agrave;y.</li>\r\n<li>Đợt 2. Gồm c&aacute;c m&atilde; dự thưởng từ ng&agrave;y&nbsp;<strong>12/11/2016</strong>&nbsp;đến&nbsp;<strong>18/11/2016.</strong>&nbsp;Quay số ng&agrave;y&nbsp;<strong>19/11/2016</strong>, chọn ra 2 kh&aacute;ch h&agrave;ng tr&uacute;ng thưởng, c&ocirc;ng bố kết quả trước 20:00 c&ugrave;ng ng&agrave;y.</li>\r\n<li>Đợt 3. Gồm c&aacute;c m&atilde; dự thưởng từ ng&agrave;y&nbsp;<strong>19/11/2016</strong>&nbsp;đến&nbsp;<strong>25/11/2016.</strong>&nbsp;Quay số ng&agrave;y&nbsp;<strong>26/11/2016</strong>, chọn ra 2 kh&aacute;ch h&agrave;ng tr&uacute;ng thưởng, c&ocirc;ng bố kết quả trước 20:00 c&ugrave;ng ng&agrave;y.</li>\r\n<li>Đợt 4. Gồm c&aacute;c m&atilde; dự thưởng từ ng&agrave;y&nbsp;<strong>26/11/2016</strong>&nbsp;đến&nbsp;<strong>30/11/2016.&nbsp;</strong>Quay số ng&agrave;y&nbsp;<strong>03/12/2016</strong>, chọn ra 1 kh&aacute;ch h&agrave;ng tr&uacute;ng thưởng, c&ocirc;ng bố kết quả trước 20:00 c&ugrave;ng ng&agrave;y.</li>\r\n</ul>\r\n<p>BTC sẽ tổ chức quay số điện tử với sự chứng kiến của c&aacute;c cơ quan truyền th&ocirc;ng v&agrave; đại diện cơ quan quản l&yacute; nh&agrave; nước c&oacute; thẩm quyền</p>\r\n<p><em>&nbsp;(*) Một kh&aacute;ch h&agrave;ng (1 CMND) chỉ được tr&uacute;ng thưởng 1 lần duy nhất trong suốt chương tr&igrave;nh, kh&aacute;ch h&agrave;ng kh&ocirc;ng tr&uacute;ng lần 1 KH&Ocirc;NG được cộng dồn sang lần sau</em></p>\r\n<p><strong><span style="text-decoration: underline;">C&Aacute;CH THỨC TRAO GIẢI</span></strong></p>\r\n<p>- Thời gian kh&aacute;ch h&agrave;ng nhận thưởng bắt đầu từ ng&agrave;y&nbsp;<strong>10/</strong><strong>12</strong><strong>/2016</strong>.</p>\r\n<p>- Tổng đ&agrave;i Thế Giới Di Động&nbsp;sẽ li&ecirc;n hệ kh&aacute;ch h&agrave;ng tr&uacute;ng thưởng trong v&ograve;ng 48 giờ kể từ ng&agrave;y tr&uacute;ng thưởng. Huỷ giải nếu li&ecirc;n lạc qu&aacute; 3 lần trong 24h kh&ocirc;ng được v&agrave; kh&ocirc;ng li&ecirc;n hệ lại tổng đ&agrave;i trong v&ograve;ng 24h.</p>\r\n<p>Mọi th&ocirc;ng tin chi tiết vui l&ograve;ng li&ecirc;n hệ tổng đ&agrave;i 1800 1060 (miễn ph&iacute; cuộc gọi từ 7h30 - 22h) để được hỗ trợ tốt nhất.</p>', 1),
(47, 'Khuyến mãi', 'Chương trình tri ân khách hàng mua Galaxy S7/S7 Edge', '<p>Chương tr&igrave;nh ưu đ&atilde;i d&agrave;nh cho kh&aacute;ch h&agrave;ng th&acirc;n thiết của Samsung khi mua Samsung Galaxy S7 hoặc Galaxy S7 Edge tại hệ thống đại l&yacute; của Samsung tr&ecirc;n to&agrave;n quốc.</p>', 'website/view/tintuc/20161110051611asus.jpg', '<p><strong>1. Ưu đ&atilde;i đặc biệt:&nbsp;</strong></p>\r\n<p>Tặng ngay bộ qu&agrave; tặng trị gi&aacute; 2.580.000 VNĐ bao gồm 1 Tai nghe Bluetooth Level Active (trị gi&aacute; 1.490.000 VNĐ) v&agrave; 1 Bao da S-View (trị gi&aacute; 1.090.000 VNĐ) khi mua điện thoại Galaxy S7 hoặc Galaxy S7 Edge với m&atilde; ưu đ&atilde;i d&agrave;nh ri&ecirc;ng cho kh&aacute;ch h&agrave;ng đang sở hữu sản phẩm Samsung ch&iacute;nh h&atilde;ng thuộc một trong c&aacute;c d&ograve;ng sản phẩm sau:&nbsp;</p>\r\n<p>- D&ograve;ng A: A9 Pro (A900), A8 (A800), A7 2016 (A710), A7 (A700), A5 2016 (A510), A5 (A500), A3 2016 (A310), A3 (A300)</p>\r\n<p>- D&ograve;ng J: J7 2016 (J710), J7 (J700), J5 2016 (J510), J5 (J500), J3 2016 (J320), J2 (J200), J1 2016 (J120), J1 (J100), J1 mini (J105)</p>\r\n<p>- D&ograve;ng Tab: Tab S2 (T819, T815, T719, T715), Tab S (T805, T705), Tab A 10.1 (T585), Tab A 9.7&rdquo; (P555), Tab A 8&rdquo; (T355, P355), Tab A6 7&rdquo; (T285), Tab E (T561), Tab 4 7&rdquo; (T231), Tab 3V (T116)</p>\r\n<p><strong>2. Hướng dẫn mua h&agrave;ng:</strong></p>\r\n<p>- Kh&aacute;ch h&agrave;ng tải v&agrave; c&agrave;i đặt ứng dụng Qu&agrave; Tặng Galaxy tr&ecirc;n thiết bị Samsung thuộc chương tr&igrave;nh m&agrave; kh&aacute;ch h&agrave;ng đang sở hữu.</p>\r\n<p>- Click v&agrave;o chương tr&igrave;nh &ldquo;Tri &acirc;n kh&aacute;ch h&agrave;ng Samsung. Tặng bộ qu&agrave; tặng đặc biệt trị gi&aacute; 2.580.000 VNĐ khi mua Samsung Galaxy S7/S7edge&rdquo;.</p>\r\n<p>- Click v&agrave;o &ldquo;Lấy m&atilde; ưu đ&atilde;i&rdquo; tr&ecirc;n ứng dụng qu&agrave; tặng Galaxy để lấy m&atilde; ưu đ&atilde;i của chương tr&igrave;nh.</p>\r\n<p>